				GetVehiclePos(GetPlayerVehicleID(i),playerinfo[i][afv][0],playerinfo[i][afv][0],playerinfo[i][afv][1]);
				if(playerinfo[i][afv][1] > playerinfo[i][afv][2])
				{
					if(++playerinfo[i][afvcheck][0] == 5)
					{
						if(!IsPlayerNearAnyObject(i) && playerinfo[i][afv][1] > playerinfo[i][afv][2] + 10.0)
						{
							playerinfo[i][afvcheck][1]++;
							if(playerinfo[i][afvcheck][1] >= 3)
							{
								playerinfo[i][afvcheck][1] = 0;
								Cheat(i,"Flying vehicle");
							}
						}
					}
					playerinfo[i][afv][2] = playerinfo[i][afv][1];
				}
			}
			dbg(101,"10");
			// Teleporting
			if((GetPlayerCheckpoint(i) == cp_bank || GetPlayerCheckpoint(i) == cp_ammu) && GetPlayerInterior(i) > 0 && IsPlayerInAnyVehicle(i)) Cheat(i,"Teleporting");
			// Reset Floods
			for(new ii=0;ii<3;ii++) playerinfo[i][flood][ii] = 0;
			// Raise Cost
			if(playerinfo[i][raise] != INVALID_PLAYER_ID)
			{
				if(playerinfo[i][upgradecmd][lcmdid("/raise")] == 1) GiveMoney(i,0-250);
				else if(playerinfo[i][upgradecmd][lcmdid("/raise")] == 2) GiveMoney(i,0-150);
				else if(playerinfo[i][upgradecmd][lcmdid("/raise")] == 3) GiveMoney(i,0-50);
				if(GetMoney(i) <= 0 || GetPlayerSpecialAction(i) != SPECIAL_ACTION_USEJETPACK) StopRaise(i);
			}
			// Charge Ultimate Power
			if(playerinfo[i][upower] != -1 && playerinfo[i][charge] > 0)
			{
				playerinfo[i][charge]++;
				valstr(string,playerinfo[i][charge]);
				format(string,sizeof(string),"~n~~n~~n~~n~~n~~n~~n~~r~ultimate power: ~b~%02d~g~~h~~h~(%s~g~~h~~h~)",playerinfo[i][charge],GetProgress(playerinfo[i][charge] / 3));
				GameTextForPlayer(i,string,900,5);
			}
			// Charge Second Ultimate Power
			if(playerinfo[i][upower2] != -1 && playerinfo[i][charge2] > 0)
			{
				playerinfo[i][charge2]++;
				valstr(string,playerinfo[i][charge2]);
				format(string,sizeof(string),"~n~~n~~n~~n~~n~~n~~n~~y~second ultimate power: ~b~%02d~g~~h~~h~(%s~g~~h~~h~)",playerinfo[i][charge2],GetProgress(playerinfo[i][charge2] / 3));
				GameTextForPlayer(i,string,900,5);
			}
			dbg(101,"11");
			// Quickly Defence
			if(playerinfo[i][quicklydefence] > 0) playerinfo[i][quicklydefence]--;
			// Tornado
			if(playerinfo[i][tornado]-- > 0)
			{
				for(new ii = 0; ii < MAX_PLAYERS_; ii++) if(IsPlayerConnected(ii))
				{
					GetPlayerPos(ii,op[0],op[1],op[2]);
					if(op[0] >= playerinfo[i][torpos][0] - 15.0 && op[0] <= playerinfo[i][torpos][0] + 15.0 &&
					op[1] >= playerinfo[i][torpos][1] - 15.0 && op[1] <= playerinfo[i][torpos][1] + 15.0 &&
					op[2] >= playerinfo[i][torpos][2] - 15.0 && op[2] <= playerinfo[i][torpos][2] + 15.0) AddPlayerVelocity(ii,floatrand(-5.0,5.0),floatrand(-5.0,5.0),floatrand(0.1,5.0));
				}
			}
			// Clan Earth Energy
			if(claninfo[playerinfo[i][clan]][cuseearthenergy])
			{
			    GetPlayerHealth(i,h);
			    if(h < 100.0) SetPlayerHealth(i,h + 5.0 > 100.0 ? 100.0 : h + 5);
			}
			dbg(101,"12");
			// Clan Godmod
			if(claninfo[playerinfo[i][clan]][cusegodmod] > 0)
			{
				claninfo[playerinfo[i][clan]][cusegodmod]--;
				SetPlayerHealth2(i,claninfo[playerinfo[i][clan]][cusegodmod] <= 1 ? 100.0 : 100000.0);
			}
			if(claninfo[playerinfo[i][clan]][cusegodmod] == 0 && claninfo[playerinfo[i][clan]][cusingpower]) claninfo[playerinfo[i][clan]][cusingpower] = 0;
			// Car Jump
			if(playerinfo[i][cancarjump] > 0) playerinfo[i][cancarjump]--;
			// Clans Alert
			hq = GetPlayerHQ(i);
			if(hq > 0 && claninfo[hq][calert][0] && claninfo[hq][calert][1] && playerinfo[i][clan] != hq && !claninfo[playerinfo[i][clan]][cpeace][hq]) for(new ii=0;ii<MAX_PLAYERS_;ii++) if(IsPlayerConnected(ii) && playerinfo[ii][clan] == hq && !playerinfo[ii][afk]) PlayerPlaySound(ii,1058,0.0,0.0,0.0);
			// Reset Copy Power
			if(playerinfo[i][copypower][2] > 0) playerinfo[i][copypower][2]--;
		}
		if(IsPlayerNPC(i))
		{
			new bool:myNPC = false;
			for(new n = 0; n < sizeof(NPCs) && !myNPC; n++) if(equal(GetName(i),NPCs[n][npcName])) myNPC = true;
			if(!myNPC) Cheat(i,"Bot");
		}
	}
	if(debugstate) printf("Contents emsa");
	// Fire place
	GetSObjectPos(fpinfo[fpUpdate]++,op[0],op[1],op[2]);
	CreateExplosion(op[0],op[1],op[2],4,0.0);
	if(fpinfo[fpUpdate] >= fpinfo[fpEnd]) fpinfo[fpUpdate] = fpinfo[fpStart];
	// Second auto message
	dbg(101,"13");
	if(++autoM >= second_auto_message_seconds*60)
	{
		new rnd = random(SAMs);
		format(string,sizeof(string)," Advertisement: %s",SecondAutoMessage[rnd]);
		SendClientMessageToAll(lightblue,string);
		autoM = 0;
	}
	// Auto Unlocking Vehicles
	dbg(101,"14");
	if(++afterHour == 3600)
	{
		for(new v = 0; v < MAX_VEHICLES; v++) if(vehicleinfo[v][vvalid]) for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i)) SetVehicleParamsForPlayer(v,i,0,0);
		SendClientMessageToAll(green," *** .ëì äîëåðéåú áùøú ðôúçå àåèåîèéú");
		afterHour = 0;
	}
	// Bot Delay
	if(++botDelay == 600)
	{
	    new pla = -1;
	    for(new i = 0; i < MAX_PLAYERS_ && pla == -1; i++) if(IsPlayerConnected(i)) pla = i;
		if(pla != -1)
		{
			BotSay(pla,BotMessages[random(botMs)]);
		    botDelay = 0;
	    }
	}
	if(debugstate) printf("Contents end");
	return 1;
}
