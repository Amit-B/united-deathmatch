//=[hash]=======================================================================
stock hash(string[])
{   // by DracoBlue, improved by me
	new s[2] = {1,0};
	for(new i = 0; i < strlen(string); i++) s[0] = (s[0] + string[i]) % 65521, s[1] = (s[1] + s[0]) % 65521;
	return (s[1] << 16) + s[0];
}
