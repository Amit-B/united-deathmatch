	if(!fexist(f)) fcreate(f);
	for(new i = 1; i < 11; i++)
	{
		format(k,sizeof(k),"BL%d",i);
		if(!fkeyexist(f,k)) fsetstring(f,k,"X");
	}
	for(new i = 9; i > 1 ; i--)
	{
		format(k,sizeof(k),"BL%d",i);
		format(string,sizeof(string),fgetstring(f,k));
		format(k,sizeof(k),"BL%d",i+1);
		fsetstring(f,k,string);
