//=[Encrypt]====================================================================
stock Encrypt(password[])
{   // by FeaR/Astro(?), improved by me
	new pass[M_S], length = strlen(password);
	for(new i=0;i<length;i++) pass[i] = password[i];
	pass[length] = 0;
	for(new i=0;i<strlen(pass);i++)
	{
		pass[i] += (3^i) * (i%15);
		if(pass[i] > (0xff)) pass[i] -= 256;
	}
	return pass;
}
