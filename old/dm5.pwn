//=[hex2rgba]===================================================================
stock hex2rgba(hex,&r,&g,&b,&a)
{
	r = (hexColor >> 24) & 0x000000FF;
	g = (hexColor >> 16) & 0x000000FF;
	b = (hexColor >> 8) & 0x000000FF;
	a = hexColor & 0x000000FF;
}
