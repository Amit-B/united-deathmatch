			if(GetPlayerState(i) == PLAYER_STATE_SPECTATING && !IsPlayerMAdmin(i) && !playerinfo[i][viewerwwe] && playerinfo[i][spectate] == -1) return Cheat(i,"Spectate hack");
			// Raise Update
			if(playerinfo[i][raise] != INVALID_PLAYER_ID)
			{
				GetPlayerPos(i,p[0],p[1],p[2]);
				SetPlayerPos(playerinfo[i][raise],p[0],p[1],p[2] - 2.0);
			}
			// Pump Update
			if(playerinfo[i][pump] != INVALID_PLAYER_ID) if(GetDistanceBetweenPlayers(i,playerinfo[i][pump]) <= (7.0 + 3.0 * playerinfo[i][upgradecmd][lcmdid("/pump")]))
			{
				Damage(playerinfo[i][pump],3.0 * playerinfo[i][upgradecmd][lcmdid("/pump")]);
				Damage(i,-3.0 * playerinfo[i][upgradecmd][lcmdid("/pump")]);
			}
