//=[CreateSpecialVehicle]=======================================================
stock CreateSpecialVehicle(modelid,Float:x,Float:y,Float:z,Float:a,color1,color2,respawn_delay=respawntime,world=world_normal,vspobj,vspid,vspname[])
{
	new v = CreateVehicleEx(modelid,x,y,z,a,color1,color2,respawn_delay,1,world);
	vehicleinfo[v][vspecial] = 1;
	vehicleinfo[v][spobject] = vspobj;
	vehicleinfo[v][vspecialid] = vspid;
	format(vehicleinfo[v][vspecialname],32,vspname);
	AttachSObjectToVehicle(vspobj,v);
	vehicleinfo[v][sv_torpedo] = 0;
	return v;
}
//=[SpawnSpecialVehicle]========================================================
stock SpawnSpecialVehicle(vehicleid)
{
	SetVehiclePos(vehicleid,vehicleinfo[vehicleid][vspawnpos][0],vehicleinfo[vehicleid][vspawnpos][1],vehicleinfo[vehicleid][vspawnpos][2]);
	SetVehicleZAngle(vehicleid,vehicleinfo[vehicleid][vspawnpos][2]);
	DetachSObject(vehicleinfo[vehicleid][spobject]);
	AttachSObjectToVehicle(vehicleinfo[vehicleid][spobject],vehicleid);
}
//=[DestroyVehicleEx]===========================================================
stock DestroyVehicleEx(v)
{
	if(!IsValidVehicle(v)) return 0;
	DestroyVehicle(v);
	vehicleinfo[v][lastdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][locked] = 0;
	vehicleinfo[v][store] = 0;
	vehicleinfo[v][storecost] = 0;
	vehicleinfo[v][racedriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][kartdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][monsterdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][vvalid] = 0;
	vehicleinfo[v][vemodel] = -1;
	return 1;
}
//=[IsValidVehicle]=============================================================
stock IsValidVehicle(v) return vehicleinfo[v][vvalid];
//=[GetTimeAsString]============================================================
stock GetTimeAsString()
{
	new t[3], s[M_S];
	gettime(t[0],t[1],t[2]);
	format(s,sizeof(s),"%d:%d",t[0],t[1]);
	return s;
}
//=[GetDateAsString]============================================================
stock GetDateAsString(tav = '/')
{
	new d[3], s[M_S];
	getdate(d[0],d[1],d[2]);
	format(s,sizeof(s),"%d%c%d%c%d",d[2],tav,d[1],tav,d[0]);
	return s;
}
//=[GetLastConnectDate]=========================================================
stock GetLastConnectDate(uname[])
{
	new string[M_S];
	format(string,sizeof(string),"/DM/Users/%s.ini",uname);
	format(string,sizeof(string),"%d/%d/%d",fgetint(string,"LastConnectDay"),fgetint(string,"LastConnectMonth"),fgetint(string,"LastConnectYear"));
	return string;
}
//=[IsValidSkin]================================================================
stock IsValidSkin(skinid)
{
	new BadSkins[] = {3,4,5,6,8,42,65,74,86,119,149,208,268,273,289};
	if(skinid < 0 || skinid > 299) return 0;
	for(new i=0;i<sizeof(BadSkins);i++) if(skinid == BadSkins[i]) return 0;
	return 1;
}
