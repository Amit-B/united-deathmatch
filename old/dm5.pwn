				SetFreeze(i,0);
				PlayerPlaySound(i,1085,0.0,0.0,0.0);
			}
			// Changed Colors
			if(playerinfo[i][cvcolor] && IsPlayerInAnyVehicle(i)) ChangeVehicleColor(GetPlayerVehicleID(i),random(127),random(127));
			// Headshot Detect
			if(Options[1][optActive])
			{
			    index = GetPlayerAnimationIndex(i);
			    if(index == 1173 || index == 1175 || index == 1177 || index == 1178)
			    {
			        index = GetPlayerWeapon(i);
			        if(index == 24 || index == 33 || index == 34)
			        {
					    SetPlayerHealth(i,0.0);
					    playerinfo[i][hshot] = 1;
				    }
			    }
		 	}
		}
	}
	return 1;
}
