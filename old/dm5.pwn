//=[weekday]====================================================================
stock weekday(day,month,year)
{
	if(month <= 2) month += 12, --year;
	new j = year % 100, e = year / 100;
	return (day + (month+1)*26/10 + j + j/4 + e/4 - 2*e) % 7;
}
