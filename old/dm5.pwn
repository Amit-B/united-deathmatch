		vehicleinfo[v][vpos][0] = fgetfloat(s,"X");
		vehicleinfo[v][vpos][1] = fgetfloat(s,"Y");
		vehicleinfo[v][vpos][2] = fgetfloat(s,"Z");
		vehicleinfo[v][vpos][3] = fgetfloat(s,"A");
		vehicleinfo[v][vcol][0] = fgetint(s,"Color1");
		vehicleinfo[v][vcol][1] = fgetint(s,"Color2");
		SetVehiclePos(v,vehicleinfo[v][vpos][0],vehicleinfo[v][vpos][1],vehicleinfo[v][vpos][2]);
		SetVehicleZAngle(v,vehicleinfo[v][vpos][3]);
		ChangeVehicleColor(v,vehicleinfo[v][vcol][0],vehicleinfo[v][vcol][1]);
	}
	LinkVehicleToInterior(v,vehicleinfo[v][veint]);
	SetVehicleVirtualWorld(v,vehicleinfo[v][veworld]);
	vehicleinfo[v][vspawnpos][0] = x;
	vehicleinfo[v][vspawnpos][1] = y;
	vehicleinfo[v][vspawnpos][2] = z;
	vehicleinfo[v][vspawnpos][3] = a;
	vehicleinfo[v][lastdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][locked] = 0;
	vehicleinfo[v][store] = 1;
	vehicleinfo[v][storecost] = cost;
	vehicleinfo[v][racedriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][kartdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][monsterdriver] = INVALID_PLAYER_ID;
	vehicleinfo[v][vvalid] = 1;
	vehicleinfo[v][vemodel] = modelid;
	vehicleinfo[v][veint] = 0;
	vehicleinfo[v][veworld] = world_normal;
	vehicleinfo[v][vclan] = 0;
	vehicleinfo[v][vspecial] = 0;
	for(new i = 0; i < 2; i++) vehicleinfo[v][newvcol][i] = vehicleinfo[v][vcol][i];
	vehicleinfo[v][storeid] = lastVStore;
	if(!cpinfo[lastVStore][vstore_first]) cpinfo[lastVStore][vstore_first] = v;
	cpinfo[lastVStore][vstore_end] = v;
	return v;
