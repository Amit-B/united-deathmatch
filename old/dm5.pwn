stock fgetstring(filename[],key[])
{
	if(debugstate) printf("fgetstring(%s,%s) start",filename,key);
	new File:fohnd, tmpres[256], tmpres2[256];
	tmpres[0] = -1;
	fohnd = fopen(filename,io_read);
