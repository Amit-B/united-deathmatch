		{
			format(k,sizeof(k),"ModSlot%d",i);
			fsetint(s,k,0);
			vehicleinfo[v][vmods][i] = 0;
		}
		vehicleinfo[v][vpos][0] = x;
		vehicleinfo[v][vpos][1] = y;
		vehicleinfo[v][vpos][2] = z;
		vehicleinfo[v][vpos][3] = a;
		vehicleinfo[v][vcol][0] = color1;
		vehicleinfo[v][vcol][1] = color2;
	}
	else
