		}
		if(playerinfo[playerid][mtime] == 1) playerinfo[playerid][mtime] = 0, m = true;
		if(m)
		{
			playerinfo[playerid][mtime] += mute_time;
			if(playerinfo[playerid][mtime] / mute_time == 3) SetKick(playerid,INVALID_PLAYER_ID,"î÷ìì");
			else
			{
