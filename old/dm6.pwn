//=[CanTeleportSVehicle]========================================================
stock CanTeleportSVehicle(playerid)
{
	if(!IsPlayerInAnyVehicle(playerid)) return 1;
	new v = GetPlayerVehicleID(playerid);
	if(!vehicleinfo[v][vspecial]) return 1;
	switch(vehicleinfo[v][vspecialid])
	{
