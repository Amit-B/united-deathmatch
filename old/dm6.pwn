	vehicleinfo[v][vclan] = 0;
	vehicleinfo[v][vrockets] = {-1,-1};
	vehicleinfo[v][vcantenter] = 0;
	vehicleinfo[v][vnpcs] = 0;
	vehicleinfo[v][vfuel] = -1;
	vehicleinfo[v][vatt] = INVALID_VEHICLE_ID;
