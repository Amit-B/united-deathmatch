public StopEarthEnergy(userclan,o1,o2,o3,o4,o5)
{
	new o = o1, Float:p[3];
	goto donetoobj;
	donetoobj:
	{
		GetSObjectPos(o,p[0],p[1],p[2]);
		MoveSObject(o,p[0],p[1],p[2]-8.0,floatrand(2.0,3.5));
		objectinfo[o][odam] = 1;
	}
	if(o != o5)
	{
		if(o == o1) o = o2;
		else if(o == o2) o = o3;
		else if(o == o3) o = o4;
		else if(o == o4) o = o5;
		goto donetoobj;
	}
	claninfo[userclan][cuseearthenergy] = 0;
	claninfo[userclan][cusingpower] = 0;
	return 1;
}
