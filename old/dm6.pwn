//=[HPText]=====================================================================
stock HPText(playerid,Float:amount,text[]="")
{
	if(!playerinfo[playerid][spawned] || playerinfo[playerid][reqclass] || floatround(amount) == playerinfo[playerid][lasthptext]) return 1;
	playerinfo[playerid][lasthptext] = floatround(amount);
	new Text:td, str[32];
	if(amount == 999.0) format(str,sizeof(str),text);
	else if(amount == 0.0) return 1;
	else if(amount > 0.0 && amount < 1.0) format(str,sizeof(str),"~%c~%.1f",amount > 0.0 ? 'g' : 'r',amount);
	else if(amount > 100.0) format(str,sizeof(str),"~g~100+");
	else format(str,sizeof(str),"~%c~%.0f",amount > 0.0 ? 'g' : 'r',amount);
	td = TextDrawCreate(146.000000,372.000000,str);
	TextDrawBackgroundColor(td,255);
	TextDrawFont(td,1);
	TextDrawLetterSize(td,0.480000,1.400000);
	TextDrawColor(td,amount > 0.0 ? green : red);
	TextDrawSetOutline(td,1);
	TextDrawSetProportional(td,1);
	ShowTempTextDraw(playerid,2000,temptext_hp,td,1);
	return 1;
}
