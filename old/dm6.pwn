		DestroyPickupEx(claninfo[clanid][cpickup][i]);
		pickupinfo[claninfo[clanid][cpickup][i]][pfunc] = 0;
		pickupinfo[claninfo[clanid][cpickup][i]][pspecialid] = 0;
		claninfo[clanid][cpickup][i] = 0;
