stock AddSObjectPath(objectid,Float:x,Float:y,Float:z,Float:movespeed)
{
	objectinfo[objectid][opath][(objectinfo[objectid][opaths]*4)] = x;
	objectinfo[objectid][opath][(objectinfo[objectid][opaths]*4)+1] = y;
	objectinfo[objectid][opath][(objectinfo[objectid][opaths]*4)+2] = z;
	objectinfo[objectid][opath][(objectinfo[objectid][opaths]*4)+3] = movespeed;
	if(!objectinfo[objectid][opaths]) MoveSObject(objectid,x,y,z,movespeed);
	objectinfo[objectid][opaths]++;
	objectinfo[objectid][odonepaths] = 0;
	return 1;
}
