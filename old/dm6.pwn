	warareasync(1);
	warareasync(2);
	warareasync(3);
	warareasync(4);
	warareasync(5);
	warareasync(6);
	warareasync(7);
	warareasync(8);
	warareasync(9);
	warareasync(10);
	warareasync(11);
	warareasync(12);
	warareasync(13);
	warareasync(14);
	#undef warareasync
