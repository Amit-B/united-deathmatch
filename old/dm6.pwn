			case 13: SetPlayerColor(i,rgba2hex(255,255,255,0));
			case 14: SetPlayerToNormalColor(i);
			case 15: playerinfo[i][acwarns] = 0;
		}
		playa[c++] = i;
	}
	if(targetid == INVALID_PLAYER_ID)
	{
		if(c == 1) SendClientMessage(playerid,green," .äôòåìä áåöòä ø÷ òì òöîê");
		else if(!c) SendClientMessage(playerid,red," .äôòåìä ìà áåöòä òì àó àçã");
		else
		{
			if(strlen(AdminAbilities[ability][aaDone]) > 0)
			{
				valstr(string,param);
				if(playerinfo[playerid][range] == 0.0)
				{
					if(param != -999) format(string,sizeof(string)," * .%sëì äùç÷ðéí áùøú " @c(white) "%s",strreplace(AdminAbilities[ability][aaDone],"X",string),GetName(playerid));
					else format(string,sizeof(string)," * .%sëì äùç÷ðéí áùøú " @c(white) "%s",AdminAbilities[ability][aaDone],GetName(playerid));
					SendClientMessageToAll(lightblue,string);
				}
				else
				{
					if(param != -999) format(string,sizeof(string)," * .%s%d äùç÷ðéí ùäéå áîøç÷ %.1f îîðå " @c(white) "%s",strreplace(AdminAbilities[ability][aaDone],"X",string),playerinfo[playerid][range],c,GetName(playerid));
					//else format(string,sizeof(string)," * .%s%d äùç÷ðéí ùäéå áîøç÷ %.1f îîðå " @c(white) "%s",AdminAbilities[ability][aaDone],playerinfo[playerid][range],c,GetName(playerid));
                    else format(string,sizeof(string),"%s%d äùç÷ðéí áîøç÷ %f îîðå %s äàãîéï",AdminAbilities[ability][aaDone],c,playerinfo[playerid][range],GetName(playerid));
					for(new i = 0; i < c; i++) SendClientMessage(playa[i],lightblue,string);
				}
			}
			else
			{
				format(string,sizeof(string)," * .äôòåìä \"%s\" áåöòä åìà ðøùîä áö'àè ìëåìí",AdminAbilities[ability][aaName]);
				SendClientMessage(playerid,lightblue,string);
			}
		}
	}
	else
	{
		format(string,sizeof(string),"%s áéöò àú äôòåìä \"%s\" òì",GetName(targetid),AdminAbilities[ability][aaName]);
		AdminInfo(playerid,string);
	}
	return 1;
}
