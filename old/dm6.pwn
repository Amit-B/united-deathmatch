//=[split]======================================================================
stock split(const strsrc[],strdest[][],delimiter)
{
	new i, li, aNum, len, len2 = strlen(strsrc);
	while(i <= len2)
	{
		if(strsrc[i] == delimiter || i == len2)
		{
			len = strmid(strdest[aNum],strsrc,li,i,128);
			strdest[aNum][len] = 0;
			li = i + 1;
			aNum++;
		}
		i++;
	}
	return 1;
}
//=[splitstring]======================================================================
stock splitstring(aExplode[][],const sSource[],const sDelimiter[]=" ",iVertices = sizeof aExplode,iLength = sizeof aExplode[])
{
    new iNode, iPointer, iPrevious = -1, iDelimiter = strlen(sDelimiter);
    while(iNode < iVertices)
    {
        iPointer = strfind(sSource, sDelimiter, false, iPointer);
        if(iPointer == -1)
    	{
            strmid(aExplode[iNode], sSource, iPrevious, strlen(sSource), iLength);
            break;
        }
        else strmid(aExplode[iNode], sSource, iPrevious, iPointer, iLength);
        iPrevious = (iPointer += iDelimiter);
        ++iNode;
    }
    return iPrevious;
}
