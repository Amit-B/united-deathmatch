		claninfo[clanid][cvehicle][i] = 0;
	}
	claninfo[clanid][cvehicles] = 0;
	for(new i = 0; i < claninfo[clanid][cpickups]; i++) if(pickupinfo[claninfo[clanid][cpickup][i]][pvalid])
