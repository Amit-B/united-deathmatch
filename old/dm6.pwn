//=[Decrypt]====================================================================
stock Decrypt(string[])
{   // Pcycho's encrypt
	new ret[32];
	format(ret,sizeof(ret),string);
	for(new i = 0, j = strlen(string); i < j; i++) ret[i] = (string[i]+64231)-12373+97864-21675-58786;
	return ret;
}
