{
	playerinfo[playerid][tooktempickup] = 0;
	switch(typ)
	{
		case 0: SetPlayerHealth(playerid,h);
		case 1: if(playerinfo[playerid][vtemp] != INVALID_VEHICLE_ID) DestroyVehicleEx(playerinfo[playerid][vtemp]);
