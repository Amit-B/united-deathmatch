		/*
		if(bandays)
		{
		    new date[6], stringt[40];
		    new bantime = gettime() + (bandays * 86400);
		    TimestampToDate(bantime,date[0],date[1],date[2],date[3],date[4],date[5],2);
		    format(stringt,sizeof(stringt),"%02d/%02d/%02d",date[2],date[1],date[0]);
