//=[Log]========================================================================
stock Log(parlogfile[],playerid,text[])
{
	if(!IsPlayerConnected(playerid) && playerid != ConsoleID) return 1;
	if(playerinfo[playerid][pp] > 0 && playerinfo[playerid][logged]) return printf("Debug LIEL TEST - AVIV");
	playerinfo[playerid][idletime] = 0;
	new string[256], date[3], logfile[32];
	if(equal(parlogfile,"LagTest"))
	{
		if(playerid != INVALID_PLAYER_ID) return 1;
		gettime2(date[0],date[1],date[1]);
		if(date[0] < 6) format(logfile,sizeof(logfile),"%s_00_05",parlogfile);
		else if(date[0] < 12) format(logfile,sizeof(logfile),"%s_06_11",parlogfile);
		else if(date[0] < 18) format(logfile,sizeof(logfile),"%s_12_17",parlogfile);
		else if(date[0] < 24) format(logfile,sizeof(logfile),"%s_18_23",parlogfile);
	}
	else format(logfile,sizeof(logfile),parlogfile);
	getdate(date[0],date[1],date[2]);
	format(string,sizeof(string),"/DM/Logs/%s/",logfile);
	folderCheck(string);
	format(string,sizeof(string),"/DM/Logs/%s/%s_%d_%d_%d.log",logfile,logfile,date[2],date[1],date[0]);
