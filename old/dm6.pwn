	vehicleinfo[v][sirenstate] = false;
	vehicleinfo[v][vemodel] = -1;
	vehicleinfo[v][vcantenter] = 0;
	vehicleinfo[v][vgm] = 0;
	vehicleinfo[v][vnpcs] = 0;
	vehicleinfo[v][tobein] = 0;
	return 1;
}
