			}
			case 4: // Insects Attack
			{
				new pl[MAX_PLAYERS], ps = 0, o, Float:po[3];
				for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && i != playerid && IsPlayerStreamedIn(i,playerid) && IsPlayerInRangeOfPoint(i,20.0,p[0],p[1],p[2])) pl[ps++] = i;
