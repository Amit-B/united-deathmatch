//=[UseAdminAbility]============================================================
stock UseAdminAbility(playerid,ability,param=-999,targetid=INVALID_PLAYER_ID)
{
	if(!IsPlayerMAdmin(playerid) || playerinfo[playerid][admin] < AdminAbilities[ability][aaLevel]) return SendClientMessage(playerid,red," .àéï ìê øîú àãîéï îñôé÷ä");
	new string[M_S], Float:p[3], in = GetPlayerInterior(playerid), wr = GetPlayerVirtualWorld(playerid), c = 0, playa[MAX_PLAYERS];
	GetPlayerPos(playerid,p[0],p[1],p[2]);
	if(targetid != INVALID_PLAYER_ID) if(!CommandAllowed(playerid,targetid,AdminAbilities[ability][aaName])) return 1;
	for(new i = 0, v = 0; i < MAX_PLAYERS_; i++) if(targetid == i || (targetid == INVALID_PLAYER_ID && IsPlayerConnected(i) && !IsPlayerNPC(i) && (playerinfo[playerid][range] == 0.0 || (playerinfo[playerid][range] != 0.0 && IsPlayerInRangeOfPoint(i,playerinfo[playerid][range],p[0],p[1],p[2])))))
	{
		switch(ability)
		{
			case 0, 1: SetFreeze(i,(_:(!ability)));
			case 2: ResetWeapons(i);
			case 3, 4:
			{
				SetPlayerInterior(i,in);
				SetPlayerVirtualWorld(i,wr);
				if(ability == 3)
				{
					GetXYInFrontOfPoint(p[0],p[1],float(random(360)),5.0);
					SetPlayerPos(i,p[0],p[1],p[2]);
					SetPlayerLookAt(i,p[0],p[1]);
				}
				else
				{
					v = GetPlayerVehicleID(i);
					GetXYInFrontOfPoint(p[0],p[1],float(random(360)),float(randomEx(8,23)));
					SetVehiclePos(v,p[0],p[1],p[2]+2.0,i);
					SetVehicleVirtualWorld(v,wr);
					LinkVehicleToInterior(v,in);
				}
			}
			case 5: SetPlayerHealth(i,0.0);
			case 6:
			{
				SetPlayerHealth(i,100.0);
				SetPlayerArmour(i,100.0);
			}
			case 7: SetPlayerHealth(i,float(param));
			case 8: SetPlayerArmour(i,float(param));
			case 9:
			{
				GiveMoney(i,param, 15);
				if(!playerinfo[playerid][range]) format(string,sizeof(string),"total (%d)%s [%d$]",i,GetName(i),param);
				else format(string,sizeof(string),"range %.1f (%d)%s [%d$]",playerinfo[playerid][range],i,GetName(i),param);
