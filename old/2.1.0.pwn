				SetPlayerFacingAngle(playerid,claninfo[playerinfo[playerid][clan]][chq][3]);
				if(jetp) GiveJetpack(playerid);
			}
			case PLAYER_STATE_DRIVER:
			{
				SetVehiclePos(GetPlayerVehicleID(playerid),claninfo[playerinfo[playerid][clan]][chqv][0]+floatrand(-3.0,3.0),claninfo[playerinfo[playerid][clan]][chqv][1]+floatrand(-3.0,3.0),claninfo[playerinfo[playerid][clan]][chqv][2],playerid);
				SetVehicleZAngle(GetPlayerVehicleID(playerid),claninfo[playerinfo[playerid][clan]][chqv][3]);
				if(GetPlayerInterior(playerid) != 0) LinkVehicleToInterior(GetPlayerVehicleID(playerid),0);
				SetVehicleVirtualWorld(GetPlayerVehicleID(playerid),0);
			}
		}
		format(string,sizeof(string)," !%s áøåê äáà ìîô÷ãú",claninfo[playerinfo[playerid][clan]][cname]);
	}
	else
	{
		PutPlayerInVehicle(playerid,vid,0);
		format(string,sizeof(string)," !%s åàì äøëá %s áøåê äáà ìîô÷ãú",GetVehicleName(GetVehicleModel(vid)),claninfo[playerinfo[playerid][clan]][cname]);
	}
	if(!playerinfo[playerid][gmtimer])
	{
		SetPlayerHealth(playerid,100000.0);
		playerinfo[playerid][gmtimer] = 2, playerinfo[playerid][gmlasthp] = h;
	}
	print("TELEHQ 4");
	SetCameraBehindPlayer(playerid);
	SetPlayerInterior(playerid,0);
	SetPlayerVirtualWorld(playerid,0);
	SendClientMessage(playerid,yellow,string);
	format(string,sizeof(string),"~y~]~h~%s ~n~~h~headquarter~y~]",claninfo[playerinfo[playerid][clan]][cname]);
	GameTextForPlayer(playerid,string,1500,4);
	return 1;
}
//=[SetVehicleComponents]=======================================================
stock SetVehicleComponents(modelid,...)
{
	new pool = GetVehiclePoolSize();
	for(new j = 0; j < pool; j++) if(IsValidVehicle(j) && vehicleinfo[j][vvalid] && vehicleinfo[j][vmodel] == modelid)
	{
		for(new i = 0; i < MAX_ for(new i = 1, m = numargs(); i < m; i++) vehicleinfo[j][vcomponent][vehicleinfo[j][vcomponents]++] = getarg(i);
		RefreshVehicleComponents(j);
	}
}
//=[RefreshVehicleComponents]===================================================
stock RefreshVehicleComponents(vehicleid)
{
	for(new i = 0, comid = -1; i < vehicleinfo[vehicleid][vcomponents]; i++)
	{
		switch(vehicleinfo[vehicleid][vcomponent][i])
		{
			case 1:
			{
				new wheels[] = {1025,1073,1074,1075,1076,1077,1078,1079,1080,1081,1082,1083,1084,1085,1096,1097,1098};
				comid = wheels[random(sizeof(wheels))];
			}
			case 2:
			{
				new nitros[] = {1008,1009,1010};
				comid = nitros[random(sizeof(nitros))];
			}
			default: comid = vehicleinfo[vehicleid][vcomponent][i];
		}
		if(comid != -1)
		{
			if(islegalcarmod(GetVehicleModel(vehicleid),comid)) AddVehicleComponent(vehicleid,comid);
			comid = -1;
		}
	}
}
//=[GetAttachedObjectPos]=======================================================
stock GetAttachedObjectPos(playerid,Float:offset_x,Float:offset_y,Float:offset_z,Float:offset_rx,Float:offset_ry,Float:offset_rz,&Float:x,&Float:y,&Float:z)
{	// by Stylock, improved by me
    new Float:p_x, Float:p_y, Float:p_z;
    GetPlayerPos(playerid,p_x,p_y,p_z);
	new Float:cos_x = floatcos(offset_rx, degrees),
		Float:cos_y = floatcos(offset_ry, degrees),
		Float:cos_z = floatcos(offset_rz, degrees),
		Float:sin_x = floatsin(offset_rx, degrees),
		Float:sin_y = floatsin(offset_ry, degrees),
		Float:sin_z = floatsin(offset_rz, degrees);
    x = p_x + offset_x * cos_y * cos_z - offset_x * sin_x * sin_y * sin_z - offset_y * cos_x * sin_z + offset_z * sin_y * cos_z + offset_z * sin_x * cos_y * sin_z;
	y = p_y + offset_x * cos_y * sin_z + offset_x * sin_x * sin_y * cos_z + offset_y * cos_x * cos_z + offset_z * sin_y * sin_z - offset_z * sin_x * cos_y * cos_z;
	z = p_z - offset_x * cos_x * sin_y + offset_y * sin_x + offset_z * cos_x * cos_y;
}
