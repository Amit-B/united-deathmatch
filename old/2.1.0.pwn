			else ConquestAreaControl(clanid,conquestid,"takeover_success");
		}
		else if(playerid == INVALID_PLAYER_ID)
		{
			if(claninfo[clanid][ctakeover][1] % 30 == 0 && claninfo[clanid][ctakeover][1] != 0)
			{
				for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && GetPlayerVirtualWorld(i) == conquestinfo[conquestid][cqworld] && playerinfo[i][inconquest] == conquestid && playerinfo[i][clan] == clanid && i != playerid) members++;
				if(members < conquestinfo[conquestid][cqreqplayers]) return ConquestAreaControl(clanid,conquestid,"takeover_failed",-2);
				else if(claninfo[clanid][ctakeover][1] % 60 == 0)
				{
