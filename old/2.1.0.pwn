stock ExecuteShowCommand(cmd[],params[])
{
	/* Available Commands:
	audio;url,x,y,z
	npc;identity,name,skin,x,y,z,angle
	delnpc;identity
	editnpc;identity,skin,x,y,z,angle
	weapon;identity,wid
