		new datetime[6];
		getdate(datetime[0],datetime[1],datetime[2]);
		gettime(datetime[3],datetime[4],datetime[5]);
		PlayerTextDrawSetString(playerid,playerinfo[playerid][border][5],GetName(playerid));
