//=[HQRulingTimer]==============================================================
stock HQRulingTimer(clanid,act,q=-1)
{
	new string[64];
	switch(act)
	{
		case 's':
		{
			if(conquestinfo[q][cqtype] == cqtype_hq) format(string,sizeof(string),"~b~~h~%s vs. %s~n~~h~%02d:00",claninfo[clanid][cname],claninfo[claninfo[clanid][cruling][0]][cname],claninfo[clanid][cruling][1]/60);
			else if(conquestinfo[q][cqtype] == cqtype_area)
			{
				switch(conquestinfo[q][cqctrl])
				{
					case cqctrl_time: format(string,sizeof(string),"~g~~h~%s~n~~h~%02d:00",conquestinfo[q][cqname],claninfo[clanid][ctakeover][1]/60);
				}
			}
			claninfo[clanid][crulingtext] = TextDrawCreate(488.000000,395.000000,string);
			TextDrawAlignment(claninfo[clanid][crulingtext],2);
			TextDrawBackgroundColor(claninfo[clanid][crulingtext],-1);
			TextDrawFont(claninfo[clanid][crulingtext],1);
			TextDrawLetterSize(claninfo[clanid][crulingtext],0.500000,1.700000);
			TextDrawColor(claninfo[clanid][crulingtext],-1);
			TextDrawSetOutline(claninfo[clanid][crulingtext],0);
			TextDrawSetProportional(claninfo[clanid][crulingtext],1);
			TextDrawSetShadow(claninfo[clanid][crulingtext],1);
			for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && (playerinfo[i][clan] == clanid || (claninfo[clanid][cruling][0] > 0 && playerinfo[i][clan] == claninfo[clanid][cruling][0]))) TextDrawShowForPlayer(i,claninfo[clanid][crulingtext]);
		}
		case 'd':
		{
			for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && (playerinfo[i][clan] == clanid || (claninfo[clanid][cruling][0] > 0 && playerinfo[i][clan] == claninfo[clanid][cruling][0]))) TextDrawHideForPlayer(i,claninfo[clanid][crulingtext]);
			TextDrawDestroy(claninfo[clanid][crulingtext]);
		}
		case 'u':
		{
			if(conquestinfo[q][cqtype] == cqtype_hq) format(string,sizeof(string),"~b~~h~%s vs. %s~n~%s%02d:%02d",claninfo[clanid][cname],claninfo[claninfo[clanid][cruling][0]][cname],claninfo[clanid][cruling][1] <= 5 ? (claninfo[clanid][cruling][1] % 2 == 0 ? ("~r~") : ("~r~~h~")) : ("~h~"),claninfo[clanid][cruling][1]/60,claninfo[clanid][cruling][1]%60);
			else if(conquestinfo[q][cqtype] == cqtype_area)
			{
				switch(conquestinfo[q][cqctrl])
				{
					case cqctrl_time: format(string,sizeof(string),"~g~~h~%s~n~%s%02d:%02d",conquestinfo[q][cqname],claninfo[clanid][ctakeover][1] <= 5 ? (claninfo[clanid][ctakeover][1] % 2 == 0 ? ("~r~") : ("~r~~h~")) : ("~h~"),claninfo[clanid][ctakeover][1]/60,claninfo[clanid][ctakeover][1]%60);
				}
			}
			TextDrawSetString(claninfo[clanid][crulingtext],string);
		}
	}
}
