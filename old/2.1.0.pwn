stock IsPlayerUsingDB(playerid,bool:withsomeone = false)
{
	if(GetState(playerid) != PLAYER_STATE_PASSENGER) return 0;
	new bool:hassomeone = false;
	if(withsomeone)
	{
		new v = GetPlayerVehicleID(playerid);
		for(new i = 0; i < MAX_PLAYERS_ && !hassomeone; i++) if(IsPlayerConnected(i) && IsPlayerInVehicle(i,v) && GetState(i) == PLAYER_STATE_DRIVER) hassomeone = true;
	}
    return GetPlayerCameraMode(playerid) == 55 && (!withsomeone || (withsomeone && hassomeone));
}
