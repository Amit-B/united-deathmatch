	new cmdType = 0, colID = -1, rgb[3] = {-1,-1,-1}, theColor = 0, s[64], idx = 0;
	for(new i = 0; i < sizeof(Colors); i++) if(equal(inputtext,Colors[i][colName])) colID = i;
