		case 4: if(playerinfo[playerid][aaobj][0] != -1)
		{
			MoveSObject(playerinfo[playerid][aaobj][0],playerinfo[playerid][aapos][0],playerinfo[playerid][aapos][1],playerinfo[playerid][aapos][2],20.0,0.0,270.0,0.0);
