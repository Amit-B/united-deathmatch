//=[Brighter]===================================================================
stock Brighter(col,to,Float:amount,alpha)
{
    new Float:cols[6];
	cols[0] = getRed(col), cols[1] = getGreen(col), cols[2] = getBlue(col), cols[3] = getRed(to), cols[4] = getGreen(to), cols[5] = getBlue(to);
	for(new i = 0, Float:d, Float:a; i < 3; i++)
	{
		d = cols[i+3] - cols[i], a = d * amount, cols[i] = cols[i] + a;
		if(cols[i] > 255) cols[i] = 255;
		if(cols[i] < 0) cols[i] = 0;
	}
    return rgba2hex(floatround(cols[0]),floatround(cols[1]),floatround(cols[2]),alpha);
}
