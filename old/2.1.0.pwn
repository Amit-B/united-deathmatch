			}
		}
	}
	if(id == -1) format(string,sizeof(string)," !%s-äùúîù á %s",GetUPLTitle(powertype,powerlevel),GetName(playerid));
	else format(string,sizeof(string)," !%s òì %s-äùúîù á %s",GetName(id),GetUPLTitle(powertype,powerlevel),GetName(playerid));
	for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && IsPlayerStreamedIn(i,playerid) && IsPlayerInRangeOfPoint(i,65.0,p[0],p[1],p[2]) && i != id)
	{
		SendClientMessage(i,UltimatePowers[playerinfo[playerid][upower]][upHex],string);
		Streamer_Update(i);
	}
	if(id != -1)
	{
		format(string,sizeof(string)," !%s äùúîù òìéê áëåç äàåìèéîèéáé %s",GetUPLTitle(powertype,powerlevel),GetName(playerid));
		SendClientMessage(id,UltimatePowers[playerinfo[playerid][upower]][upHex],string);
		Streamer_Update(id);
	}
