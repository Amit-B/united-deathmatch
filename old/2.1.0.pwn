//=[BorderAlpha]================================================================
stock BorderAlpha(playerid,type)
{
	if(!playerinfo[playerid][borderalpha]) return 0;
	switch(type)
	{
		case 0: // first line
			switch(playerinfo[playerid][borderalpha])
			{
				case 1: return 75;
				case 2: return 125;
				case 3: return 175;
			}
		case 1: // second line
			switch(playerinfo[playerid][borderalpha])
			{
				case 1: return 50;
				case 2: return 100;
				case 3: return 150;
			}
		case 2: // seperators
			switch(playerinfo[playerid][borderalpha])
			{
				case 1: return 175;
				case 2: return 225;
				case 3: return 255;
			}
	}
	return 0;
}
