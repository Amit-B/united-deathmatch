//=[IsValidNick]================================================================
stock IsValidNick(name[])
{
	new len = strlen(name);
	if(len < 3 || len > 20 || equal(name,"None") || equal(name,botname)) return 0;
	for(new i = 0; i < len; i++)
	{
		if(!(name[i] >= '0' && name[i] <= '9')
		&& !(name[i] >= 'A' && name[i] <= 'Z')
		&& !(name[i] >= 'a' && name[i] <= 'z')
		&& name[i] != '_' && name[i] != '[' && name[i] != ']'
		&& name[i] != '.' && name[i] != '(' && name[i] != ')'
		&& name[i] != '@' && name[i] != '$') return 0;
	}
	return 1;
}
