//=[Tune]=======================================================================
stock Tune(playerid,bool:tune)
{
	new vehicleid = GetPlayerVehicleID(playerid);
	if(tune)
	{
		if(playerinfo[playerid][tuning]) return 0;
		new modelid = GetVehicleModel(vehicleid), tuningshopid = GetVehicleModelTuningShop(modelid);
		if(tuningshopid == -1) return 0;
		playerinfo[playerid][tuning] = true;
		GetVehiclePos(vehicleid,playerinfo[playerid][tuningpos][0],playerinfo[playerid][tuningpos][1],playerinfo[playerid][tuningpos][2]);
		GetVehicleZAngle(vehicleid,playerinfo[playerid][tuningpos][3]);
		if(vehiclePassengers[vehicleid] > 0) for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && i != playerid && GetPlayerState(i) != PLAYER_STATE_DRIVER && GetPlayerVehicleID(i) == vehicleid)
		{
			SetPlayerPos(i,playerinfo[playerid][tuningpos][0],playerinfo[playerid][tuningpos][1],playerinfo[playerid][tuningpos][2]);
			SetPlayerFacingAngle(i,playerinfo[playerid][tuningpos][3]);
		}
		new Float:p[4];
		GetTuningShopPosition(tuningshopid,p[0],p[1],p[2],p[3]);
		SetVehiclePos(vehicleid,p[0],p[1],p[2]);
		SetVehicleZAngle(vehicleid,p[3]);
		if(GetPlayerVehicleID(playerid) != vehicleid || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) PutPlayerInVehicle(playerid,vehicleid,0);
		CancelSelectTextDraw(playerid);
		HidePlayerDialog(playerid);
	}
	else
	{
		SetVehiclePos(vehicleid,playerinfo[playerid][tuningpos][0],playerinfo[playerid][tuningpos][1],playerinfo[playerid][tuningpos][2]);
		SetVehicleZAngle(vehicleid,playerinfo[playerid][tuningpos][3]);
		playerinfo[playerid][tuning] = false;
		for(new i = 0; i < 4; i++) playerinfo[playerid][tuningpos][i] = 0.0;
	}
	return 1;
}
