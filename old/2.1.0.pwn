{
	new fq[64], fc[64], string[M_S];
	format(fq,sizeof(fq),folder_conquests "%d.ini",id);
	format(fc,sizeof(fc),folder_clans "%s.ini",claninfo[clanid][cname]);
	switch(occupytype)
	{
		case 0: // unoccupy
		{
			if(conquestinfo[id][cqtype] == cqtype_area)
			{
				for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && playerinfo[i][clan] == clanid && playerinfo[i][activatingconquest][0] == id) ConquestAbility(clanid,id,i,'c',"äàæåø ëáø ìà ùééê ì÷ìàï");
				if(equal(conquestinfo[id][cqcode],cqcode_army))
				{
					new c = GetConquestControlCount(clanid,cqcode_army);
					for(new i = 0; i < 4; i++) if(claninfo[clanid][carmyveh][i] != -1 && c < ArmyConquest[i][1])
					{
						format(string,sizeof(string)," .ùæéîðúí ðîç÷ ëé éù ìëí ëòú îòè îãé àæåøéí öáàééí áùìéèúëí %s äøëá äöáàé",GetVehicleName(ArmyConquest[i][0]));
						ClanNotification(clanid,string);
						DestroyVehicleEx(claninfo[clanid][carmyveh][i]);
					}
				}
			}
			conquestinfo[id][cqowner] = 0;
			conquestinfo[id][cqcooldown] = 0;
			if(conquestinfo[id][cqtype] == cqtype_hq)
			{
				claninfo[clanid][chqid] = -1;
				claninfo[clanid][cshqid] = -1;
				claninfo[clanid][hashq] = 0;
				if(conquestinfo[id][cqloaded])
				{
					for(new i = 0; i < conquestinfo[id][cqobjects]; i++) ResetSObjectMaterial(conquestinfo[id][cqobject][i]);
					for(new i = 0, k[16], v = 0; i < conquestinfo[id][cqvehicles]; i++)
					{
						format(k,sizeof(k),"Vehicle%d",i);
						v = conquestinfo[id][cqvehicle][i];
						format(string,sizeof(string),"%d,%.4f,%.4f,%.4f,%.4f,%d,%d",vehicleinfo[v][vemodel],vehicleinfo[v][vspawnpos][0],vehicleinfo[v][vspawnpos][1],vehicleinfo[v][vspawnpos][2],vehicleinfo[v][vspawnpos][3],vehicleinfo[v][newvcol][0] = -1,vehicleinfo[v][newvcol][1] = -1);
						fsetstring(fq,k,string);
						ChangeVehicleColor(v,-1,-1);
					}
				}
			}
			if(notif)
			{
				format(string,sizeof(string)," * %s :ä÷ìàï ëáø ìà ááòìåú òì äàæåø",conquestinfo[id][cqname]);
				ClanNotification(clanid,string);
			}
			fsetint(fq,"Owner",0);
		}
		case 1, 2: // occupy, buy
		{
			if(occupytype == 2)
			{
				if(claninfo[clanid][cbank] < conquestinfo[id][cqprice]) return 0;
				claninfo[clanid][cbank] -= conquestinfo[id][cqprice];
				fsetint(fc,"Bank",claninfo[clanid][cbank]);
			}
			conquestinfo[id][cqowner] = clanid;
			if(conquestinfo[id][cqtype] == cqtype_hq)
			{
				claninfo[clanid][chqid] = id;
				claninfo[clanid][cshqid] = id;
				claninfo[clanid][hashq] = 1;
				fsetfloat(fc,"X",claninfo[clanid][chq][0] = conquestinfo[id][cqpos][0]);
				fsetfloat(fc,"Y",claninfo[clanid][chq][1] = conquestinfo[id][cqpos][1]);
				fsetfloat(fc,"Z",claninfo[clanid][chq][2] = conquestinfo[id][cqpos][2]);
				fsetfloat(fc,"Rot",claninfo[clanid][chq][3] = conquestinfo[id][cqpos][3]);
				fsetfloat(fc,"VX",claninfo[clanid][chqv][0] = conquestinfo[id][cqpos][0]);
				fsetfloat(fc,"VY",claninfo[clanid][chqv][1] = conquestinfo[id][cqpos][1]);
				fsetfloat(fc,"VZ",claninfo[clanid][chqv][2] = conquestinfo[id][cqpos][2]);
				fsetfloat(fc,"VR",claninfo[clanid][chqv][3] = conquestinfo[id][cqpos][3]);
			}
			if(notif)
			{
				format(string,sizeof(string)," * %s :ä÷ìàï %s àú äàæåø",conquestinfo[id][cqname],occupytype == 2 ? ("÷ðä") : ("ëáù ëòú"));
				ClanNotification(clanid,string);
			}
			fsetint(fq,"Owner",clanid);
		}
	}
	if(conquestinfo[id][cqloaded])
	{
		GangZoneHideForAll(conquestinfo[id][cqzone]);
		GangZoneShowForAll(conquestinfo[id][cqzone],ConquestGangZoneColor(id));
		format(string,sizeof(string),GetConquestName(id));
		for(new i = 0; i < conquestinfo[id][cqvehicles]; i++)
		{
			SetVehicleNumberPlate(conquestinfo[id][cqvehicle][i],string);
			SetVehicleToRespawn(conquestinfo[id][cqvehicle][i]);
		}
	}
	if(conquestinfo[id][cqtype] == cqtype_hq) UpdateHQList();
	return 1;
}
