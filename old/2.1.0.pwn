stock hash(string[])
{   // by DracoBlue, improved by me
	new s[2] = {1,0};
	for(new i = 0, l = strlen(string); i < l; i++) s[0] = (s[0] + string[i]) % 65521, s[1] = (s[1] + s[0]) % 65521;
