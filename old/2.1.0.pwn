	if(playerinfo[playerid][selectingpos][0] > 0 && playerinfo[playerid][clan] > 0 && claninfo[playerinfo[playerid][clan]][cvalid])
	{
		new Float:p[3], bool:sf = false;
		if(newkeys & KEY_NO)
		{
			if(playerinfo[playerid][selectingpos][0] == 3) sf = true;
			playerinfo[playerid][selectingpos][0] = -1, playerinfo[playerid][selectingpos][1] = 0;
			ClanDialog(playerid,clan_not,@c(green) ".áéèìú àú áçéøú äîé÷åí",playerinfo[playerid][selectingpos][2]);
			PlaySound(playerid,1085);
		}
		else if(newkeys & KEY_YES && (playerinfo[playerid][selectingpos][0] == 1 || playerinfo[playerid][selectingpos][0] == 3))
		{
			if(playerinfo[playerid][selectingpos][0] == 3) sf = true;
			else GetPlayerPos(playerid,p[0],p[1],p[2]);
			DoneSelectingPos(playerid,p[0],p[1],p[2]);
		}
		if(sf)
		{
			StopFlyMode(playerid,p[0],p[1],p[2]);
			SetPlayerPos(playerid,p[0],p[1],p[2]);
			playerinfo[playerid][flytype] = fmt_none;
		}
	}
	if(playerinfo[playerid][flymode][0] && playerinfo[playerid][flytype] == fmt_admin)
	{
		new Float:p[3];
		if(newkeys & KEY_SPRINT)
		{
			StopFlyMode(playerid,p[0],p[1],p[2]);
			SetPlayerPos(playerid,p[0],p[1],p[2]);
			playerinfo[playerid][flytype] = fmt_none;
		}
		else SendClientMessage(playerid,grey," (~k~~PED_SPRINT~ ìñéåí ìçõ)");
	}
	if(playerinfo[playerid][lasteupdate][1] > 0 && GetState(playerid) == PLAYER_STATE_ONFOOT && (newkeys & KEY_SPRINT || newkeys & KEY_JUMP)) playerinfo[playerid][lasteupdate][1] = 0;
	if(playerinfo[playerid][beingspectated] && !playerinfo[playerid][pp])
	{
		format(string,sizeof(string),"[KEY] %s (ID: %03d): %s",GetName(playerid),playerid,GetKeyNames(playerid,newkeys));
		for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && playerinfo[i][spectate] == playerid && IsPlayerMAdmin(i)) SendClientMessage(i,0x66CCCC,string);
	}
	if(newkeys & KEY_FIRE && playerinfo[playerid][vehicle][0] && IsPlayerInVehicle(playerid,playerinfo[playerid][vehicle][1]) && GetVehicleVirtualWorld(playerinfo[playerid][vehicle][1]) != world_personalvehicles && playerinfo[playerid][pvspcomp][vmod_nitro] && !IllegalCarNitroIde(GetVehicleModel(playerinfo[playerid][vehicle][1]))) UpdateNitro(playerinfo[playerid][vehicle][1],playerid,playerinfo[playerid][pvspcomp][vmod_nitro]);
	playerinfo[playerid][pkeys][0] = newkeys, playerinfo[playerid][pkeys][1] = oldkeys;
	#if lagtest
		LagTest_EndEx("OnPlayerKeyStateChangeH(%d)",newkeys);
	#endif
	if(debugstate) printf("OnPlayerKeyStateChange(%s(%d),%d,%d) end",GetName(playerid),playerid,newkeys,oldkeys);
	return 1;
}
