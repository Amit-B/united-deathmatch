//=[CheckForInvaders]===========================================================
stock CheckForInvaders(clanid)
{
	assert claninfo[clanid][cvalid];
	new player[MAX_PLAYERS], players = 0, string[M_S], bool:clans[MAX_CLANS] = {false,...}, vtypes[6] = {false,...}, vtc = 0, cs = 0;
	for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && playerinfo[i][clan] != clanid && GetPlayerHQ(i) == clanid && !claninfo[clanid][cpeace][playerinfo[i][clan]]) player[players++] = i;
	if(players > 0)
	{
		if(players == 1) format(string,sizeof(string),"æåää ôåìù àçã");
		else format(string,sizeof(string),"æåäå %d ôåìùéí",players);
		for(new j = 0, i = INVALID_PLAYER_ID, vt = -1; j < players; j++)
		{
			i = player[j];
			if(playerinfo[i][clan] > 0 && !clans[playerinfo[i][clan]]) clans[playerinfo[i][clan]] = true, cs++;
			if(IsPlayerInAnyVehicle(i))
			{
				vt = GetVehicleType(GetPlayerVehicleID(i));
				if(!vtypes[vt]) vtypes[vt] = true, vtc++;
			}
		}
		if(vtc > 0)
		{
			for(new i = 0, c = 0, v[16]; i < sizeof(vtypes); i++) if(vtypes[i])
			{
				c++;
				switch(i)
				{
					case 0: format(v,sizeof(v),"îëåðéú");
					case 1: format(v,sizeof(v),"àåôðåò");
					case 2: format(v,sizeof(v),"ñéøä");
					case 3: format(v,sizeof(v),"äìé÷åôèø");
					case 4: format(v,sizeof(v),"îèåñ");
					default: format(v,sizeof(v),"øëá ìà éãåò");
				}
				if(c == 1) format(string,sizeof(string),"îùúîùéí á%s",v);
				if(c == 2) format(string,sizeof(string),"%s åâí %s",string,v);
				else format(string,sizeof(string),"%s, %s",string,v);
			}
		}
		else format(string,sizeof(string),"%s %s",string,players == 1 ? ("ëçééì øâìé") : ("ëçééìéí øâìééí"));
		if(cs > 0)
		{
			format(string,sizeof(string),"%s %s",string,cs == 1 ? ("îä÷ìàï") : ("îä÷ìàðéí"));
			for(new i = 0, c = 0; i < MAX_CLANS; i++) if(clans[i] && claninfo[i][cvalid])
			{
				format(string,sizeof(string),!c ? ("%s %s") : ("%s, %s"),claninfo[i][cname],string);
				c++;
			}
		}
	}
	format(string,sizeof(string)," [%s Security] %s",claninfo[clanid][cname],string);
	ClanNotification(clanid,string);
	return 1;
}
