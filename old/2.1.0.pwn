		claninfo[clanid][ctakeover][0] = conquestid, claninfo[clanid][ctakeover][1] = conquestinfo[conquestid][cqctrlparam];
		claninfo[clanid][ctakeover][2] = SetTimerEx("ConquestAreaRuling",1000,1,"ii",clanid,conquestid);
		claninfo[clanid][cbusy] = true;
		HQRulingTimer(clanid,'s',conquestid);
	}
	else if(equal(option,"retreat"))
