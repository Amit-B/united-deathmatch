stock CheckNewBestScore(playerid,bestscoreid,amount,bool:upper)
{
	if(!amount) return;
	if((upper && (amount > playerinfo[playerid][bestscore][bestscoreid] || !playerinfo[playerid][bestscore][bestscoreid])) ||
	(!upper && (amount < playerinfo[playerid][bestscore][bestscoreid] || !playerinfo[playerid][bestscore][bestscoreid])))
	{
		playerinfo[playerid][bestscore][bestscoreid] = amount;
		new string[M_S];
		if(playerinfo[playerid][logged])
		{
			format(string,sizeof(string),"BestScore%d",bestscoreid);
			fsetint(fuser(playerid),string,amount);
		}
		format(string,sizeof(string),"  !/stats-á Weapons äùéà äçãù ðøùí àöìê áúôøéè !%s-ùáøú àú äùéà ùìê á",GetBestScoreName(bestscoreid));
		SendClientMessage(playerid,orange,string);
		if((upper && (amount > serverDetails[sdBestScore][bestscoreid] || !serverDetails[sdBestScore][bestscoreid])) ||
		(!upper && (amount < serverDetails[sdBestScore][bestscoreid] || !serverDetails[sdBestScore][bestscoreid])))
		{
			format(string,sizeof(string),"sdBestScore%02d",bestscoreid);
			fsetint(file_srvdetails,string,amount);
			format(string,sizeof(string),"sdBestScore%02dName",bestscoreid);
			fsetstring(file_srvdetails,string,GetName(playerid));
			format(string,sizeof(string),"  [%s] !%s-ùáø àú äùéà ùì äùøú á %s ",GetBestScoreFormat(bestscoreid,amount),GetBestScoreName(bestscoreid),GetName(playerid));
		}
	}
}
