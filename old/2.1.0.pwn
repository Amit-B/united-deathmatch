			{
				new vw = GetPlayerVirtualWorld(playerid);
				for(new i = 0; i < claninfo[clanid][cmembers]; i++) if(playerinfo[claninfo[clanid][cplayers][i]][inconquest] == conquestid && CanUseCommand(claninfo[clanid][cplayers][i],true) && GetPlayerVirtualWorld(claninfo[clanid][cplayers][i]) == vw) playa[playas++] = claninfo[clanid][cplayers][i];
				if(!playas) return ConquestAbility(clanid,conquestid,playerid,'c',"àéï ùç÷ðéí");
				for(new i = 0; i < playas; i++)
				{
					SetPlayerVirtualWorld(playa[i],world_normal);
					SetPlayerPos(playa[i],playerinfo[playerid][smappos4conquest][0],playerinfo[playerid][smappos4conquest][1],playerinfo[playerid][smappos4conquest][2]);
					SetPlayerInterior(playa[i],0);
					LoadPlayerWeapons(playa[i]);
					SetPlayerArmedWeapon(playa[i],46);
				}
			}
			playerinfo[playerid][activatingconquest] = {-1,0,0};
		}
		case 'c': // cancel
		{
			if(playerinfo[playerid][activatingconquest][0] != -1)
			{
				playerinfo[playerid][activatingconquest] = {-1,0,0};
				format(string,sizeof(string)," * .áåèìä: %s %s äôòìú äéëåìú ùì äàæåø",reason,conquestinfo[conquestid][cqname]);
				ClanNotification(clanid,string);
			}
		}
		case 'r': // ready
		{
			for(new i = 0; i < claninfo[clanid][cmembers]; i++) if(playerinfo[claninfo[clanid][cplayers][i]][inconquest] == conquestid && CanUseCommand(claninfo[clanid][cplayers][i],true)) playa[playas++] = claninfo[clanid][cplayers][i];
			if(!playas) return ConquestAbility(clanid,conquestid,playerid,'c',"àéï ùç÷ðéí");
			if(equal(conquestinfo[conquestid][cqcode],cqcode_airport))
			{
				new vw = GetEmptyWorld();
				for(new i = 0; i < playas; i++)
				{
					SetPlayerVirtualWorld(playa[i],vw);
					SetPlayerPos(playa[i],1.5627,23.1112 + floatrand(0.0,10.0),1199.5938);
					SetPlayerFacingAngle(playa[i],358.8634);
					SetPlayerInterior(playa[i],1);
					SavePlayerWeapons(playa[i]);
					ResetWeapons(playa[i]);
					GiveWeapon(playa[i],46,0);
				}
			}
			playerinfo[playerid][activatingconquest][2] = -1, playerinfo[playerid][activatingconquest][1] = 6;
		}
	}
	return 1;
}
//=[CheckLevelRequirement]======================================================
stock CheckLevelRequirement(playerid,req,bool:msg = true)
{
	new string[M_S], bool:send = false;
	if(!playerinfo[playerid][logged])
	{
		if(msg) SendClientMessage(playerid,red," :òìéê ìäéåú øùåí åîçåáø ìùøú òì îðú ìáöò àú äôòåìä");
		send = true;
	}
	else if(playerinfo[playerid][level] < LevelRequirements[req][lvlreqLevel])
	{
		if(msg)
		{
			format(string,sizeof(string)," :òìéê ìäéåú ìôçåú áøîä %d òì îðú ìáöò àú äôòåìä",LevelRequirements[req][lvlreqLevel]);
