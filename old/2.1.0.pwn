	if(strlen(msg) > 0) format(fullchat,sizeof(fullchat),"%s\n" @c(red) "---\n%s",fullchat,msg);
	format(tmp,sizeof(tmp),"{%s}ö'àè: %s",HexStringOfColor(chatinfo[playerid][chatColor]),chatinfo[playerinfo[playerid][inchat]][chatTitle]);
