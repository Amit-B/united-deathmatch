//=[HidePet]====================================================================
stock HidePet(playerid)
{
	if(playerinfo[playerid][pet][2])
	{
		if(playerinfo[playerid][pet][3]) StopPetAttacking(playerid);
		if(IsValidObject(playerinfo[playerid][pet][1])) DestroyObject(playerinfo[playerid][pet][1]);
		playerinfo[playerid][pet][2] = 0;
		playerinfo[playerid][pet][3] = 0;
		if(playerinfo[playerid][pet][5] != -1) if(petOutside[playerinfo[playerid][pet][5]] == playerid)
		{
			for(new i = playerinfo[playerid][pet][5]; i < petsOutside; i++) petOutside[i] = petOutside[i+1];
			petOutside[--petsOutside] = -1;
		}
		playerinfo[playerid][pet][5] = -1;
		UpdatePetNameTag(playerid);
	}
}
