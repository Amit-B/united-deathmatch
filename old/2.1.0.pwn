//=[LoadConquestElements]=======================================================
stock LoadConquestElements(id,bool:updatehqlist = true)
{
	#if lagtest
		LagTest_Start("LoadConquestElements");
	#endif
	new string[M_S], key[32], keyid = 0, tmp[128], vid, vnumplate[33];
	format(string,M_S,folder_conquests "%d.ini",id);
	print("--- Loading Objects");
	format(key,sizeof(key),"Object%d",keyid);
	while(fkeyexist(string,key))
	{
		Reset2DString(clanObject);
		format(tmp,sizeof(tmp),fgetstring(string,key));
		split(tmp,clanObject,',');
		//printf("%s = %s",key,tmp);
		if(strlen(clanObject[7]) > 0) conquestinfo[id][cqobject][conquestinfo[id][cqobjects]++] = CreateSObject_CQ(id,strval(clanObject[0]),floatstr(clanObject[1]),floatstr(clanObject[2]),floatstr(clanObject[3]),floatstr(clanObject[4]),floatstr(clanObject[5]),floatstr(clanObject[6]),1,floatstr(clanObject[7]),floatstr(clanObject[8]),floatstr(clanObject[9]),!strlen(clanObject[10]) ? 5.0 : floatstr(clanObject[10]));
		else conquestinfo[id][cqobject][conquestinfo[id][cqobjects]++] = CreateSObject_CQ(id,strval(clanObject[0]),floatstr(clanObject[1]),floatstr(clanObject[2]),floatstr(clanObject[3]),floatstr(clanObject[4]),floatstr(clanObject[5]),floatstr(clanObject[6]));
		keyid++;
		format(key,sizeof(key),"Object%d",keyid);
		for(new j = 0; j < 3; j++) conquestinfo[id][cqavgpos][j] += floatstr(clanObject[j+1]);
	}
	for(new j = 0; j < 3; j++) conquestinfo[id][cqavgpos][j] /= conquestinfo[id][cqobjects];
	keyid = 0;
	print("--- Loading Vehicles");
	format(key,sizeof(key),"Vehicle%d",keyid);
	format(vnumplate,sizeof(vnumplate),GetConquestName(id));
	while(fkeyexist(string,key))
	{
		Reset2DString(clanVehicle);
		format(tmp,sizeof(tmp),fgetstring(string,key));
		split(tmp,clanVehicle,',');
		conquestinfo[id][cqvehicle][conquestinfo[id][cqvehicles]++] = CreateVehicleEx_CQ(strval(clanVehicle[0]),floatstr(clanVehicle[1]),floatstr(clanVehicle[2]),floatstr(clanVehicle[3]),floatstr(clanVehicle[4]),strval(clanVehicle[5]),strval(clanVehicle[6]),respawntime,id);
		SetVehicleNumberPlate(vid,GetConquestName(id));
		keyid++;
		format(key,sizeof(key),"Vehicle%d",keyid);
	}
	keyid = 0;
	print("--- Loading Pickups");
	format(key,sizeof(key),"Pickup%d",keyid);
	while(fkeyexist(string,key))
	{
		Reset2DString(clanPickup);
		format(tmp,sizeof(tmp),fgetstring(string,key));
		split(tmp,clanPickup,',');
		conquestinfo[id][cqpickup][conquestinfo[id][cqpickups]++] = CreatePickupEx_CQ(strval(clanPickup[0]),floatstr(clanPickup[1]),floatstr(clanPickup[2]),floatstr(clanPickup[3]),id);
		keyid++;
		format(key,sizeof(key),"Pickup%d",keyid);
	}
	print("--- Loading Area");
	conquestinfo[id][cqarea] = CreateArea(conquestinfo[id][cqpos][0]-conquestinfo[id][cqsize][0],conquestinfo[id][cqpos][1]-conquestinfo[id][cqsize][1],conquestinfo[id][cqpos][0]+conquestinfo[id][cqsize][0],conquestinfo[id][cqpos][1]+conquestinfo[id][cqsize][1],area_conquest,id,0,conquestinfo[id][cqworld]);
	print("--- Loading Zone");
	conquestinfo[id][cqzone] = GangZoneCreate(conquestinfo[id][cqpos][0]-conquestinfo[id][cqsize][0],conquestinfo[id][cqpos][1]-conquestinfo[id][cqsize][1],conquestinfo[id][cqpos][0]+conquestinfo[id][cqsize][0],conquestinfo[id][cqpos][1]+conquestinfo[id][cqsize][1]);
	print("--- Loading Owner");
	if(conquestinfo[id][cqowner] > 0 && conquestinfo[id][cqtype] == cqtype_hq)
	{
		claninfo[conquestinfo[id][cqowner]][chqid] = id;
		if(!updatehqlist) claninfo[conquestinfo[id][cqowner]][cshqid] = id;
		claninfo[conquestinfo[id][cqowner]][hashq] = (conquestinfo[id][cqobjects] + conquestinfo[id][cqvehicles] + conquestinfo[id][cqpickups]) > 0;
	}
	print("--- Loading GangZone Color");
	GangZoneShowForAll(conquestinfo[id][cqzone],ConquestGangZoneColor(id));
	print("--- Loading Teleports");
	conquestinfo[id][cqclosetele] = -1;
	for(new t = 0, Float:dist = 500.0; t < sizeof(Teleports); t++) if(!Teleports[t][Ti] && Teleports[t][Tw] == world_normal && Teleports[t][Ttype] == tele_normal && DistanceToPoint(conquestinfo[id][cqpos][0],conquestinfo[id][cqpos][1],conquestinfo[id][cqpos][2],Teleports[t][Tx],Teleports[t][Ty],Teleports[t][Tz]) < dist)
	{
		conquestinfo[id][cqclosetele] = t;
		dist = DistanceToPoint(conquestinfo[id][cqpos][0],conquestinfo[id][cqpos][1],conquestinfo[id][cqpos][2],Teleports[t][Tx],Teleports[t][Ty],Teleports[t][Tz]);
	}
	print("--- Loading List");
	if(updatehqlist) UpdateHQList();
	conquestinfo[id][cqloaded] = true;
	#if lagtest
		LagTest_End("LoadConquestElements");
	#endif
	return 1;
}
//=[UnloadConquestElements]=====================================================
stock UnloadConquestElements(id)
{
	#if lagtest
		LagTest_Start("UnloadConquestElements");
	#endif
	for(new i = 0; i < conquestinfo[id][cqobjects]; i++) DestroySObject(conquestinfo[id][cqobject][i]);
	for(new i = 0; i < conquestinfo[id][cqvehicles]; i++) DestroyVehicleEx(conquestinfo[id][cqvehicle][i]);
	for(new i = 0; i < conquestinfo[id][cqpickups]; i++) DestroyPickupEx(conquestinfo[id][cqpickup][i]);
	DestroyArea(conquestinfo[id][cqarea]);
	GangZoneHideForAll(conquestinfo[id][cqzone]);
	GangZoneDestroy(conquestinfo[id][cqzone]);
	if(conquestinfo[id][cqowner] > 0) if(conquestinfo[id][cqtype] == cqtype_hq) claninfo[conquestinfo[id][cqowner]][chqid] = -1, claninfo[conquestinfo[id][cqowner]][hashq] = 0;
	conquestinfo[id][cqclosetele] = -1;
	UpdateHQList();
	conquestinfo[id][cqloaded] = false;
	#if lagtest
		LagTest_End("UnloadConquestElements");
	#endif
}
