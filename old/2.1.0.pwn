		if(!claninfo[clanid][ctakeover][1])
		{
			for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && GetPlayerVirtualWorld(i) == conquestinfo[conquestid][cqworld] && playerinfo[i][inconquest] == conquestid && playerinfo[i][clan] == clanid && i != playerid) member[members++] = i;
