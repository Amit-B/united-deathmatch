	object;model,x,y,z,rx,ry,rz,duration
	mobject;model,x,y,z,rx,ry,rz,nx,ny,nz,nrx,nry,nrz,speed,duration
	boom;expid,x,y,z,shake
	health;amount
	velocity;x,y,z
	velocityme;identity,x,y,z
