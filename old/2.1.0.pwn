//=[NumberPlateText]============================================================
stock NumberPlateText(text[])
{
	new ret[64], newret[64], bool:exp;
	format(ret,sizeof(ret),text);
	newret = "";
	for(new i = 0, len = strlen(ret); i < len && strlen(newret) < 32; i++)
	{
		exp = (ret[i] >= 65 && ret[i] <= 90) || (ret[i] >= 97 && ret[i] <= 122) || (ret[i] >= 48 && ret[i] <= 57);
		if(!exp && (!i || i == len-1)) continue;
		format(newret,sizeof(newret),"%s%c",newret,exp ? ret[i] : ' ');
	}
	return newret;
}
