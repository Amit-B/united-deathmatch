		return SendClientMessage(playerid,red,string);
	}
	playerinfo[playerid][teleportedhq] = 1;
	print("TELEHQ 2");
	if(strlen(cmd) > 2)
	{
		new vmodel = IsNumeric(cmd) ? strval(cmd) : GetVehicleModelIDFromName(cmd);
		if(vmodel < 400 || vmodel > 611) vid = -1;
		else for(new i = 0, Float:vp[3]; i < conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqvehicles] && vid == -1; i++) if(GetVehicleModel(conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqvehicle][i]) == vmodel && vehicleDriver[conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqvehicle][i]] == INVALID_PLAYER_ID)
		{
			GetVehiclePos(conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqvehicle][i],vp[0],vp[1],vp[2]);
			if(!IsPointInDynamicArea(conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqarea],vp[0],vp[1],vp[2])) continue;
			vid = conquestinfo[claninfo[playerinfo[playerid][clan]][chqid]][cqvehicle][i];
