					for(new i = 0; i < MAX_PLAYERS_; i++) if(playerinfo[i][clan] == clanid || playerinfo[i][inconquest] == conquestid) SendClientMessage(i,lightgreen,string);
				}
			}
			claninfo[clanid][ctakeover][1]--;
			HQRulingTimer(clanid,'u',conquestid);
		}
	}
	else if(equal(option,"takeover_success"))
	{
		OccupyConquestArea(conquestid,clanid,1,true);
		format(string,sizeof(string)," -> !%s äöìéç ìäùúìè òì àæåø ääùúìèåú {%s}%s" @c(lightgreen) " ä÷ìàï <-",conquestinfo[conquestid][cqname],ClanHexColor(clanid),claninfo[clanid][cname]);
