public WinKickstar()
{
	if(debugstate) printf("Gamemode Activity [start]: WinKickstar");
	#if lagtest
		LagTest_Start("WinKickstar");
	#endif
	new string[M_S];
	kickstarinfo[4]--;
	if(!kickstarinfo[4])
	{
