//=[SaveAdminData]==============================================================
stock SaveAdminData(playerid,dataname[],amount,bool:sta_ = false)
{
	if(IsPlayerMAdmin(playerid))
	{
		new f[64], prev = 0;
		format(f,sizeof(f),folder_infoad "%s-%s.ini",GetName(playerid),GetDateAsString('-'));
		fileCheck(f);
		if(!sta_)
		{
			if(fkeyexist(f,dataname)) prev = fgetint(f,dataname);
			fsetint(f,dataname,amount+prev);
		}
		else if(!fkeyexist(f,dataname)) fsetint(f,dataname,amount);
	}
}
