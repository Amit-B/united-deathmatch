//=[DisplayName]================================================================
stock DisplayName(string[])
{
	new string_[M_S];
	format(string_,sizeof(string_),string);
	for(new i = 0, len = strlen(string); i < sizeof(string) && i < len; i++)
	{
		if(string[i] == '[') string_[i] = '(';
		else if(string[i] == ']') string_[i] = ')';
		else if(string_[i] == '@') continue;
		else string_[i] = string[i];
	}
	return string_;
}
