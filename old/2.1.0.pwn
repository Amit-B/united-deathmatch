			{
				SetVehiclePos(playerinfo[playerid][aaveh][0],playerinfo[playerid][aapos][0]-5.0,playerinfo[playerid][aapos][1]+playerinfo[playerid][aasetp],playerinfo[playerid][aapos][2]+57.0);
