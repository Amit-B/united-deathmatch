//=[GetDaysInMonth]=============================================================
stock GetDaysInMonth(month,year)
{
	new daylist[] = {31,28,31,30,31,30,31,31,30,31,30,31};
	return daylist[month-1] + _:(month == 2 && IsLeapYear(year));
