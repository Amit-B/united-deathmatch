//=[LoadVehicleSpecialComponent]================================================
stock LoadVehicleSpecialComponent(playerid,vehicleid,bool:load,id = -1,status = 0) // status: 0 = load/unload component, 1 = initially load component, 2 = buy/sell component
{
	for(new i = 1; i < max_vmod; i++) if(playerinfo[playerid][pvspcomp][i] && (id == -1 || id == i)) switch(i)
	{
		case vmod_defense: if(load || (!load && status == 2)) SetVehicleHealth(vehicleid,GetVehicleMHealth(GetVehicleModel(vehicleid),playerid));
		case vmod_fuel: if(load || (!load && status == 2)) playerinfo[playerid][fuel] = min(playerinfo[playerid][fuel],GetVehicleFuel(playerinfo[playerid][vehicle][0],playerid));
		case vmod_nitro:
		{
			new nitroid = playerinfo[playerid][pvspcomp][i] + 1007, current = GetVehicleComponentInSlot(vehicleid,5);
			if(nitroid >= 1008 && nitroid <= 1010)
			{
				if(load)
				{
					if(status >= 1) AddVehicleComponent(vehicleid,nitroid,playerid);
					else if(current != nitroid)
					{
						if(current != 0) RemoveVehicleComponent(vehicleid,current);
						AddVehicleComponent(vehicleid,nitroid,playerid);
					}
				}
				else if(status == 2 && current == nitroid) RemoveVehicleComponent(vehicleid,current);
			}
		}
		case vmod_tracker:
		{
			if(load)
			{
				playerinfo[i][trackershow] = false;
				if(status >= 1)
				{
					playerinfo[playerid][gpsshow][0] = true;
					SendClientMessage(playerid,lightgreen," [GPS]" @c(green) " /gps :îåëðä ìùéîåù, ðà ìáçåø îé÷åí ãøê äîôä ëãé ìðååè àå äùúîù áô÷åãä äáàä ëãé ìëáåú GPS-îòøëú ä");
				}
			}
			else if(status == 2 && playerinfo[playerid][gpsshow][1])
			{
				if(IsValidPlayerObject(playerid,playerinfo[playerid][gpsobject])) DestroyPlayerObject(playerid,playerinfo[playerid][gpsobject]);
				playerinfo[playerid][gpsobject] = -1;
			}
		}
		case vmod_platetxt, vmod_platecol:
		{
			if(id == vmod_platetxt && !playerinfo[playerid][pvspcomp][id] && !equal(playerinfo[playerid][numberplatetxt],"0") && !load && status == 2)
			{
				format(playerinfo[playerid][numberplatetxt],32,"0");
				if(playerinfo[playerid][logged]) fsetstring(fuser(playerid),"NumberPlate","0");
			}
			else if(id == vmod_platetxt && load && status == 2) ShowPlayerDialog(playerid,dialog_numberplatetxt,DIALOG_STYLE_INPUT,@c(lightblue) "ùéðåé è÷ñè ìåçéú øéùåé",@c(white) ":ðà ìä÷ìéã è÷ñè òã 8 úåéí òí àåúéåú áàðâìéú åîñôøéí áìáã","àéùåø","áéèåì");
			else UpdateVehicleNumberPlate(playerid,vehicleid);
		}
		case vmod_wheels:
		{
			new model = -1, current = GetVehicleComponentInSlot(vehicleid,7);
			switch(playerinfo[playerid][pvspcomp][i])
			{
				case 1: model = 1080;
			}
			if(model != -1)
			{
				if(load)
				{
					if(status >= 1) AddVehicleComponent(vehicleid,model,playerid);
