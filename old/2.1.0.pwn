	}
	else if(equal(option,"abandon"))
	{
		OccupyConquestArea(conquestid,clanid,0,true);
		format(string,sizeof(string)," .%s äåôñ÷ä òì éãé %s ääùúìèåú ùì ä÷ìàï ùìê òì äàæåø",GetName(playerid),conquestinfo[conquestid][cqname]);
		for(new i = 0; i < MAX_PLAYERS_; i++) if(IsPlayerConnected(i) && playerinfo[i][clan] == clanid) SendClientMessage(i,green," .îô÷ãú ä÷ìàï ùìê ùåçøøä îääùúìèåú");
