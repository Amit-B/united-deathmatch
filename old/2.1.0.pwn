	new string[M_S];
	switch(type)
	{
		case 0:
		{
			if(amount > GetMoney(playerid))
			{
				format(string,sizeof(string)," .áåèìä ëé àéï ìê éåúø àú äëñó %s äòáøú ñëåí äëñó $%d àì",GetName(id),amount);
				SendClientMessage(playerid,red,string);
				format(string,sizeof(string)," .áåèìä ëé àéï ìå àú äëñó éåúø %s äòáøú ñëåí äëñó $%d àìéê òì éãé äîùúîù",GetName(playerid),amount);
				SendClientMessage(id,red,string);
			}
			else if(playerinfo[id][dmzone] != dmz_none || playerinfo[playerid][tdmzone] != tdmz_none)
			{
				format(string,sizeof(string)," .áåèìä ëé äåà ðëðñ ìàæåø ãé-àí %s äòáøú ñëåí äëñó $%d àì",GetName(id),amount);
				SendClientMessage(playerid,red,string);
				format(string,sizeof(string)," .áåèìä ëé ðëðñú ìàæåø ãé-àí %s äòáøú ñëåí äëñó $%d àìéê òì éãé äîùúîù",GetName(playerid),amount);
				SendClientMessage(id,red,string);
			}
			else
			{
				GiveMoney(playerid,0-amount);
				GiveMoney(id,amount);
				SendClientMessage(playerid,yellow," .ùìéçú äëñó äåùìîä");
				SendClientMessage(id,yellow," .ùìéçú äëñó äåùìîä");
			}
		}
		case 1:
		{
			assert ((IsPlayerMAdmin(playerid) && playerinfo[playerid][admin] >= 10) || PlayerHasRank(playerid,RankID("äøùàåú îåâáøåú")));
			format(string,sizeof(string)," .%s ùìçú %d$ ì",GetName(id),amount);
			SendClientMessage(playerid,green,string);
			format(string,sizeof(string)," .÷éáìú îäàãîéï %d$",amount);
			SendClientMessage(id,green,string);
			format(string,sizeof(string),"via /gm: money sent (%d)%s [%d$]",id,GetName(id),amount);
			Log("Attention",playerid,string);
			GiveMoney(id,amount);
		}
	}
	playerinfo[playerid][paytimer] = {0,INVALID_PLAYER_ID,-1};
	return 1;
