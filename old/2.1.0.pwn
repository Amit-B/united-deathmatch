//=[strhex]=====================================================================
stock strhex(string[])
{   // by Y_Less, improved by me
	new ret, val, i;
	if(string[0] == '0' && (string[1] == 'x' || string[1] == 'X')) i = 2;
	while(string[i])
	{
		ret <<= 4, val = (string[i++] - '0');
		if(val > 0x09) val -= 0x07;
		if(val > 0x0F) val -= 0x20;
		if(val < 0x01) continue;
		if(val < 0x10) ret += val;
	}
	return ret;
}
//=[IsHex]======================================================================
stock IsHex(string[])
{   // by Y_Less, improved by me
	new i, cur;
	if(string[0] == '0' && (string[1] == 'x' || string[1] == 'X')) i = 2;
	while(string[i])
	{
		cur = string[i++];
		if((cur < '0') || (cur > '9' && cur < 'A') || (cur > 'F' && cur < 'a') || (cur > 'f')) return false;
	}
	return 1;
}
