//=[CallPlayerVehicle]==========================================================
stock CallPlayerVehicle(playerid,index,request)
{
	new Float:p[4], bool:flag = false;
	switch(index)
	{
		case 1:
		{
			switch(request)
			{
				case 1:
				{
					if(playerinfo[playerid][vehicle][1] == -1)
					{
						GetPlayerPos(playerid,p[0],p[1],p[2]);
						GetPlayerFacingAngle(playerid,p[3]);
						playerinfo[playerid][vehicle][1] = CreateVehicleEx(playerinfo[playerid][vehicle][0],p[0],p[1],p[2],p[3],playerinfo[playerid][pvcol][0],playerinfo[playerid][pvcol][1],respawntime,0,world_normal);
						vehicleinfo[playerinfo[playerid][vehicle][1]][store] = 1;
						vehicleinfo[playerinfo[playerid][vehicle][1]][vowner] = playerid;
						LoadVehicleSpecialComponent(playerid,playerinfo[playerid][vehicle][1],true,.status = 1);
					}
					else
					{
						if(vehicleDriver[playerinfo[playerid][vehicle][1]] != INVALID_PLAYER_ID)
						{
							GetPlayerPos(vehicleDriver[playerinfo[playerid][vehicle][1]],p[0],p[1],p[2]);
							SetPlayerPos(vehicleDriver[playerinfo[playerid][vehicle][1]],p[0],p[1],p[2]+2.0);
							SendClientMessage(vehicleDriver[playerinfo[playerid][vehicle][1]],red," !áòì äøëá æéîï àú äøëá ùìå");
						}
						SetVehicleVirtualWorld(playerinfo[playerid][vehicle][1],world_normal);
						LinkVehicleToInterior(playerinfo[playerid][vehicle][1],0);
						GetPlayerPos(playerid,p[0],p[1],p[2]);
						GetPlayerFacingAngle(playerid,p[3]);
						SetVehiclePos(playerinfo[playerid][vehicle][1],p[0],p[1],p[2]);
						SetVehicleZAngle(playerinfo[playerid][vehicle][1],p[3]);
						LoadVehicleSpecialComponent(playerid,playerinfo[playerid][vehicle][1],true);
					}
					ChangeVehicleColor(playerinfo[playerid][vehicle][1],playerinfo[playerid][pvcol][0],playerinfo[playerid][pvcol][1]);
					if(playerinfo[playerid][pvpj] > 0) ChangeVehiclePaintjob(playerinfo[playerid][vehicle][1],playerinfo[playerid][pvpj]-1);
					for(new i = 0; i <= 13; i++) if(playerinfo[playerid][pvmod][i] > 0) if(islegalcarmod(GetVehicleModel(GetPlayerVehicleID(playerid)), playerinfo[playerid][pvmod][i])) AddVehicleComponent(playerinfo[playerid][vehicle][1],playerinfo[playerid][pvmod][i],playerid);
					if(playerinfo[playerid][fuel] >= 0) SetVehicleParams(playerinfo[playerid][vehicle][1],.engine = (_:(playerinfo[playerid][fuel] > 0)));
					RepairVehicle(playerinfo[playerid][vehicle][1],playerid);
					SetTimerEx("PutInVehicle",1000,0,"iii",playerid,playerinfo[playerid][vehicle][1],0);
				}
				case 2:
				{
					if(IsPlayerInVehicle(playerid,playerinfo[playerid][vehicle][1])) flag = true;
					GetPlayerPos(playerid,p[0],p[1],p[2]);
					SetVehiclePos(playerinfo[playerid][vehicle][1],floatrand(-2000.0,2000.0),floatrand(-2000.0,2000.0),666.0);
					SetVehicleVirtualWorld(playerinfo[playerid][vehicle][1],world_personalvehicles);
					if(flag) SetPlayerPos(playerid,p[0],p[1],p[2]);
					RepairVehicle(playerinfo[playerid][vehicle][1],playerid);
				}
				case 3:
				{
					LoadVehicleSpecialComponent(playerid,playerinfo[playerid][vehicle][1],false);
					DestroyVehicleEx(playerinfo[playerid][vehicle][1]);
					playerinfo[playerid][vehicle][1] = -1;
				}
			}
		}
		case 2:
		{
			switch(request)
			{
				case 1:
				{
					if(playerinfo[playerid][vehicle][8] == -1)
					{
						GetPlayerPos(playerid,p[0],p[1],p[2]);
						GetPlayerFacingAngle(playerid,p[3]);
						playerinfo[playerid][vehicle][8] = CreateVehicleEx(playerinfo[playerid][vehicle][3],p[0],p[1],p[2],p[3],playerinfo[playerid][pvcol2][0],playerinfo[playerid][pvcol2][1],respawntime,0,world_normal);
						vehicleinfo[playerinfo[playerid][vehicle][8]][store] = 1;
						vehicleinfo[playerinfo[playerid][vehicle][8]][vowner] = playerid;
					}
					else
					{
						if(vehicleDriver[playerinfo[playerid][vehicle][8]] != INVALID_PLAYER_ID)
						{
