//=[HackCheck]==================================================================
forward HackCheck(playerid,lvl);
public HackCheck(playerid,lvl)
{
	switch(lvl)
	{
		//case 10..7: wait! (4freeze)
		case 6: // frozen camera test
		{
		    new Float:p[3];
		    GetPlayerCameraFrontVector(playerid, p[0], p[1], p[2]);
		    if(p[2] < -0.8)
			{
		        SetFreeze(playerid,0);
		        playerinfo[playerid][hacker][0] = 1;
		    }
		}
		case 5: // hide camera & create vehicle
		{
			SavePlayerPos(playerid);
			SetPlayerCameraPos(playerid,-2648.078857,-46.293476,6.335937);
			SetPlayerCameraLookAt(playerid,-2648.078857,-41.293476,6.335937);
		}
		case 4: // put in vehicle
		{
			playerinfo[playerid][hackvehiclecheck] = CreateVehicleEx(457,0.0,0.0,4.0,0.0,-1,-1,respawntime,0,GetPlayerVirtualWorld(playerid))
			PutPlayerInVehicle(playerid,playerinfo[playerid][hackvehiclecheck],0);
			SetFreeze(playerid,1);
			SetPlayerCameraPos(playerid,-2648.078857,-46.293476,6.335937);
			SetPlayerCameraLookAt(playerid,-2648.078857,-41.293476,6.335937);
		}
		case 3: // vehicle check & destroy
		{
			if(PlayerHasWeapon(playerid,2)) playerinfo[playerid][hacker][1] = 1;
			if(IsValidVehicle(playerinfo[playerid][hackvehiclecheck])) DestroyVehicleEx(playerinfo[playerid][hackvehiclecheck]);
		}
		case 2: // fix camera
		{
			//LoadPlayerPos(playerid); // naor
			SpawnPlayer(playerid);
			SetCameraBehindPlayer(playerid);
			SetPlayerVirtualWorld(playerid,world_normal);
		}
		case 1: // warning / ban
		{
			if(playerinfo[playerid][hacker][0] || playerinfo[playerid][hacker][1])
			{
		        new s[128];
		        if(playerinfo[playerid][logged])
		        {
			        new year, month, day, formattedstring[11];
					getdate(year, month, day);
				 	format(formattedstring,sizeof(formattedstring),"%02d/%02d/%d", day, month, year);
			        if(equal(playerinfo[playerid][s0beitdate1],""))
			        {
				        format(s,sizeof(s),"S0beit detected - first warning, code A%d-B%d",playerinfo[playerid][hacker][0],playerinfo[playerid][hacker][1]);
			        	Log("s0beit",playerid,s);
			        	fsetstring(fuser(playerid),"s0beitdate1",formattedstring);
			        	playerinfo[playerid][s0beitdate1] = formattedstring;
			        }
			        else if(equal(playerinfo[playerid][s0beitdate2],""))
			        {
			        	if(DaysBetweenDates(playerinfo[playerid][s0beitdate1],GetDateAsString()) >= 90)
			        	{
					        format(s,sizeof(s),"S0beit detected - second warning, however, more than 3 months passed since the first warning, setting first warning a new date, code A%d-B%d",playerinfo[playerid][hacker][0],playerinfo[playerid][hacker][1]);
				        	Log("s0beit",playerid,s);
		    	        	fsetstring(fuser(playerid),"s0beitdate1",formattedstring);
			        		playerinfo[playerid][s0beitdate1] = formattedstring;
			        	}
			        	else
			        	{
					        format(s,sizeof(s),"S0beit detected - second warning, code A%d-B%d",playerinfo[playerid][hacker][0],playerinfo[playerid][hacker][1]);
				        	Log("s0beit",playerid,s);
				        	fsetstring(fuser(playerid),"s0beitdate2",formattedstring);
				        	playerinfo[playerid][s0beitdate2] = formattedstring;
			        	}
			        }
			        else
			        {
			       		fsetstring(fuser(playerid),"s0beitdate3",formattedstring);
			        	playerinfo[playerid][s0beitdate3] = formattedstring;
			        	Log("s0beit",playerid,"S0beit detected - third warning, banning in 30 seconds...");
			        	if(lagstatus <= 100) playerinfo[playerid][s0beitbantimer] = SetTimerEx("s0beitban",30000,0,"d",playerid);//SetTimerPck(playerinfo[playerid][s0beitbantimer],"s0beitban",30000,0,"d",playerid);
			        }
		        }
		        else
		        {
		       		Log("s0beit",playerid,"S0beit detected - guest account, banning in 30 seconds...");
		        	if(lagstatus <= 100) playerinfo[playerid][s0beitbantimer] = SetTimerEx("s0beitban",30000,0,"d",playerid);//SetTimerPck(playerinfo[playerid][s0beitbantimer],"s0beitban",30000,0,"d",playerid);
		        }
		        format(s,sizeof(s),"Suspected cheater - code %d-%d",playerinfo[playerid][hacker][0],playerinfo[playerid][hacker][1]);
	        	AdminInfo(playerid,s);
	        }
		}
	    case 0: // done
		{
			SetFreeze(playerid,0);
		}
	}
	if(lvl > 1 && (playerinfo[playerid][hacker][0] || playerinfo[playerid][hacker][1])) playerinfo[playerid][hackchecktimer] = 2; // to be 1 then warning...
    return 1;
}
