//=[OnPlayerChatCommand]========================================================
public OnPlayerChatCommand(playerid,cmdtext[])
{
	new cmd[256], idx;
	cmd = strtok(cmdtext,idx);
	if(equal(cmd,"&help") || equal(cmd,"&cmd"))
	{
		SendClientMessage(playerid,lightblue," ~~~ :ô÷åãåú ö'àè ~~~");
		SendClientMessage(playerid,yellow," &help / &cmd - äúôøéè äæä");
		SendClientMessage(playerid,yellow," &copy - äòú÷ú ääåãòä äàçøåðä îäö'àè");
		SendClientMessage(playerid,yellow," &paste - äãá÷ú ääåãòä ùäòú÷ú");
		SendClientMessage(playerid,yellow," &chars - øùéîú úåéí îéåçãéí");
		return 1;
	}
	if(equal(cmd,"&copy"))
	{
		if(strlen(lastChatText) > 180) return SendClientMessage(playerid,red," .ääåãòä äàçøåðä ùðøùîä áö'àè àøåëä îãé");
		if(!strlen(lastChatText)) return SendClientMessage(playerid,red," .ìà ðøùîä ùåí äåãòä áö'àè / äàãîéï ðé÷ä àú äö'àè, ìëï ìà ðéúï ìäòúé÷");
		format(playerinfo[playerid][copytext],200,lastChatText);
		SendClientMessage(playerid,green," :äòú÷ú àú äè÷ñè äáà");
		SendClientMessage(playerid,white,lastChatText);
		SendClientMessage(playerid,green," &paste ìäãá÷ä øùåí");
		if(IsPlayerUsingSGuard_(playerid)) // guard_shit
		{
			SendClientMessage(playerid,white," Ctrl+C äåòú÷ âí ì");
			SetClipboardText(playerid,lastChatText);
		}
		return 1;
	}
	if(equal(cmd,"&paste"))
	{
		if(!strlen(playerinfo[playerid][copytext])) return SendClientMessage(playerid,red," .ìà äòú÷ú ùåí äåãòä");
		SendClientMessage(playerid,green," :äè÷ñè ùäòú÷ú äåãá÷ áö'àè");
		OnPlayerText(playerid,playerinfo[playerid][copytext]);
		return 1;
	}
	if(equal(cmd,"&chars"))
	{
		SendClientMessage(playerid,lightblue," ~~~ :àôùøåéåú ö'àè ~~~");
		format(cmd,sizeof(cmd)," 1) àçåæ = %s","%%");
		SendClientMessage(playerid,yellow,cmd);
		for(new i = 0; i < sizeof(chatKeywords); i++)
		{
			format(cmd,sizeof(cmd)," %d) %s = %s",i+2,chatKeywords[i][ckWord],chatKeywords[i][ckReplaceTo]);
			SendClientMessage(playerid,yellow,cmd);
		}
		return 1;
	}
	return 0;
}
